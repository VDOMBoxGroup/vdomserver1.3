class VDOM_formdate(VDOM_object):

	def render(self, parent, contents=""):
		display = u"display: none;" if self.visible == "0" else u""

		if self.mode == "1":
			mode = u"""readonly="readonly" """
		elif self.mode == "2":
			mode = u"""disabled="disabled" """
		else:
			mode = u""
				
		auto = u"off" if self.autocomplete == "0" else u"on"

		clean_id = (self.id).replace('-', '_')
		id = u"o_%s" % clean_id

		# for doctype xhtml 1.0 - width|height depends on padding
		fixw = int(self.width) - 10
		fixh = int(self.height) - 4

		if self.inputmode == "1":
			js = u"""<script type='text/javascript'>$q(function($){
				$j('#%(id)s').datepicker('destroy').datepicker({ dateFormat: 'yy-mm-dd' });
				/*$j("#ui-datepicker-div").css("z-index", "9999" );*/
			});</script>""" % { "id": id }
			modestyle = u"background:url('/0f94d563-04ee-0283-2138-62c68af01145.res') right 50% no-repeat;"
		else:
			js = u""
			modestyle = u""

		style = u"""z-index: {zind}; position: {pos}; top: {top}px; left: {left}px; width: {width}px; height: {height}px;
					padding: 2px 5px; {display} font: 14px tahoma; {modestyle}
				""".format(zind = self.zindex, pos = self.position, top = self.top, left = self.left, 
						width = fixw, height = fixh, display = display, modestyle = modestyle)

		result = u"""<input id="{id}" ver="2011-10-12" name="{name}" style="{style}" type="text" tabindex="{tabind}" 
					value="{value}" class="{classn}" autocomplete="{auto}" {mode} /> {js}
			""".format(id = id, name = self.name, style = style, tabind = self.tabindex, 
						value = self.value, classn = self.classname, auto = auto, mode = mode, js = js)

		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		text_size = self.width if self.value else 0
		img_x = int(self.width) - 18
		if img_x < 0: img_x = 2
		if self.inputmode == "1":
			modestyle = u"""<image x="{img_x}" y="2" width="16" height="16" href="#Res(0f94d563-04ee-0283-2138-62c68af01145)" />""".format(
				img_x = img_x )
		else:
			u""
		result = u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}" 
							top="{top}" left="{left}" width="{width}" height="{height}">
						<svg>
							<rect x="0" y="0" width="{rec_wid}" height="{rec_hei}" fill="#FFFFFF" stroke="#000000"/>
							{modestyle}
						</svg>
						<text top="4" width="{txt_wid}" color="#000000">{value}</text>
					</container>
				 """.format(id = self.id, vis = self.visible, zind = self.zindex, 
						hierarchy = self.hierarchy, order = self.order, modestyle = modestyle,
						top = self.top, left = self.left, width = self.width, height = self.height, 
						rec_wid = int(self.width) - 1, rec_hei = int(self.height) - 1,
						txt_wid = text_size, value = self.value )
		
		return VDOM_object.wysiwyg(self, parent, contents=result)

		]]></SourceCode>
