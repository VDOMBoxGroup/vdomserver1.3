class VDOM_breadcrumbs(VDOM_object):

	def __mk_ref(self, oid, txt):

		color_number = self.linkcolor if self.linkcolor else u"000000"
		style = u""" style="color: #{color_number}" """.format( color_number = color_number )

		return """<a href="/{oid}.vdom" {style}>{text}</a> """.format( oid = oid, style = style, text = txt )

	def get_value (self) :
		top = application.objects.search(self.id)
		while top.parent:
			top = top.parent
		st = application.structure
		lvl = 0
		try:
			lvl = int(self.level)
		except: pass
		if lvl < 0 or lvl > 9:
			lvl = 0
		st_lv = None
		try:
			st_lv = st[str(lvl)]
		except: pass

		oid = top.id
		result = []
		while st_lv:
			if oid not in st_lv.level_map: break
			if oid != top.id:
				result.insert(0, self.__mk_ref(oid, application.objects.search(oid).attributes.title))
			else:
				result.insert(0, application.objects.search(oid).attributes.title)
			if 0 == len(st_lv.level_map[oid].parents): break
			oid = st_lv.level_map[oid].parents[0].id
			if oid == top.id: break

		res = ""
		if 0 == len(result): res = top.attributes.title
		else: res = " &gt; ".join(result)

		return res

	def render(self,parent, contents=""):
		mainstyle = \
			u"""z-index:{zindex}; position:{position}; top:{top}px; left:{left}px;
				width:{width}px; overflow:visible; {display}
				font: {fontsize}px {fontfamily}; color: #{textcolor};
			""".format(
					zindex = self.zindex,  position = self.position,
					hierarchy = self.hierarchy, order = self.order,
					top = self.top, left = self.left, width = self.width,
					display = u"display:none;" if self.visible == "0" else u"",
					fontsize = self.fontsize, fontfamily = self.fontfamily.replace('"', "'"),
					textcolor = self.textcolor)

		out = u"""<div id="{id}" style="{mainstyle}">{value}</div>""".format(
			id = 'o_' + (self.id).replace('-', '_'),
			mainstyle = mainstyle,
			value = self.get_value())

		return VDOM_object.render(self, parent, contents=out)

	def wysiwyg(self, parent, contents=""):
		style = u""" font: {fontsize}px {fontfamily}; color: #{textcolor}; """.format(
			fontsize = self.fontsize,
			fontfamily = self.fontfamily.replace('"', "'"),
			textcolor=self.textcolor)

		value = self.get_value()

		out = u"""<div style="{style}">{value}</div>""".format(value=value, style=style)

		result = \
			u"""<container id="{id}" visible="{visible}" zindex="{zindex}" hierarchy="{hierarchy}"
							order="{order}" top="{top}" left="{left}" width="{width}" height="{height}">
					<htmltext width="{width}" locked="true">
						{out}
					</htmltext>
				</container>
			""".format(
					id = self.id, visible = self.visible,
					zindex = self.zindex, hierarchy = self.hierarchy,
					order = self.order,
					top = self.top, left = self.left,
					width = self.width, height = self.height,
					out = out)

		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]></SourceCode>
