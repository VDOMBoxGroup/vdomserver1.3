  <SourceCode><![CDATA[

class VDOM_breadcrumbs(VDOM_object):

	def __mk_ref(self, oid, txt):
		style = ""
		if self.linkcolor:
			style = 'style="color: #%s"' % self.linkcolor
		return "<a href=\"/%s.vdom\" %s>%s</a>" % (oid, style, txt)

	def render(self,parent, contents=""):
		display = u"display:none;" if self.visible == "0" else u""
		font = ""
		mainstyle="z-index: %s;position: %s;top: %spx;left: %spx; width: %spx; overflow: visible;%s"%(self.zindex, self.position, self.top, self.left, self.width, display)
		mainstyle += " font: %spx %s;" % (self.fontsize, self.fontfamily)
		if self.textcolor:
			mainstyle += " color: #%s;" % self.textcolor
		top = application.objects.search(self.id)
		while top.parent:
			top = top.parent
		st = application.structure
		lvl = 0
		try:
			lvl = int(self.level)
		except: pass
		if lvl < 0 or lvl > 9:
			lvl = 0
		st_lv = None
		try:
			st_lv = st[str(lvl)]
		except: pass

		oid = top.id
		result = []
		while st_lv:
			if oid not in st_lv.level_map: break
			if oid != top.id:
				result.insert(0, self.__mk_ref(oid, application.objects.search(oid).attributes.title))
			else:
				result.insert(0, application.objects.search(oid).attributes.title)
			if 0 == len(st_lv.level_map[oid].parents): break
			oid = st_lv.level_map[oid].parents[0].id
			if oid == top.id: break

		res = ""
		if 0 == len(result): res = top.attributes.title
		else: res = " &gt; ".join(result)

		id = 'o_' + (self.id).replace('-', '_')
		
		out = u"<div id='%s' style='%s'>%s</div>" % (id, mainstyle, res)
		return VDOM_object.render(self, parent, contents=out)

	def wysiwyg(self, parent, contents=""):
		top = application.objects.search(self.id)
		while top.parent:
			top = top.parent
		st = application.structure
		lvl = 0
		try:
			lvl = int(self.level)
		except: pass
		if lvl < 0 or lvl > 9:
			lvl = 0
		st_lv = None
		try:
			st_lv = st[str(lvl)]
		except: pass

		oid = top.id
		result = []
		while st_lv:
			if oid not in st_lv.level_map: break
			if oid != top.id:
				result.insert(0, application.objects.search(oid).attributes.title)
			else:
				result.insert(0, application.objects.search(oid).attributes.title)
			if 0 == len(st_lv.level_map[oid].parents): break
			oid = st_lv.level_map[oid].parents[0].id
			if oid == top.id: break

		res = ""
		if 0 == len(result): res = top.attributes.title
		else: res = " &gt; ".join(result)

		result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
				"<text top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
				"%s</container>"%(
					self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, self.height,
					0, 0, self.width, self.height, self.fontfamily, self.fontsize, res, 
					contents)
		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]></SourceCode>
