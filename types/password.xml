def on_compile(application, object, action_name, context, objects):
	result=objects
	for xobject in object.get_objects_list():
		result.append({"object": xobject})
	return result

class VDOMNativePassword(VDOM_object):

	def render(self, parent, contents=""):
		import e2vdom
		
		e2vdom.process(self, parent)
		user_id = -1
			
		args = request.arguments
		if "password" in args and "login" in args:
			login = args["login"]
			password = args["password"]
								
			from database.dbobject import VDOM_sql_query
			query = """ SELECT web_user.id
						FROM web_user
						WHERE web_user.login='%s' AND web_user.password='%s' """%(login, password)
			q = VDOM_sql_query(application.id, self.database, query)
			res = q.fetchall()
				
			if len(res) > 0:
				user_id = res[0][0]
				session["SecurityCode"] = user_id
				ref_obj = application.objects.search(self.target)
				page = "/%s.vdom" % ref_obj.name if ref_obj else ""
				response.redirect(page)
				return u""

		if int(user_id) == -1 or not "password" in args or not "login" in args:
			obj = application.objects.search(self.id)
			obj_parent = obj.parent
			while (obj_parent.type.name != "htmlcontainer"):
				obj_parent = obj_parent.parent
			ref_obj = application.objects.search(obj_parent.id)
			target = "/%s.vdom" % ref_obj.name if ref_obj else ""
				
		display = u"display:none;" if self.visible == "0" else u""
		style = u"""overflow:auto; z-index:{zind}; position:{pos}; 
					top:{top}px; left:{left}px; width:{width}px; height:{height}px; {display}
				""".format(zind = self.zindex, pos = self.position, top = self.top, left = self.left, 
							width = self.width, height = self.height, display = display)
		id = u"o_" + (self.id).replace('-', '_')

		result = u"""<form id="{id}" name="{name}" style="{style}" method="post" action="{target}" 
						enctype="application/x-www-form-urlencoded">{contents}</form>"""\
				.format(id = id, name = self.name, style = style, target = target, contents = contents)

		return VDOM_object.render(self, parent, contents=result)


	def wysiwyg(self, parent, contents=""):
		obj = application.objects.search(self.id)
		
		childs = obj.objects
		pwd = lgn = 0
		
		for id in childs:
			object = application.objects.search(id)
			if object.name == "login" and object.type.name == "formtext":
				lgn = lgn + 1
			if object.name == "password" and object.type.name == "formpassword":
				pwd = pwd + 1
			
		if lgn == 1 and pwd == 1:
			error_mes = u""
		else:
			error_mes = u"""Necessary objects with names "login" and "password" are absent."""
		
		result = u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}" 
							top="{top}" left="{left}" width="{width}" height="{height}">
						<svg>
							<rect x="0" y="0" width="{rec_wid}" height="{rec_hei}" fill="#EEEEEE"/>
						</svg>
						<text top="5" left="5" width="200" height="40" color="#FF1010">{error}</text>{contents}
					</container>
				""".format(id = self.id, vis = self.visible, zind = self.zindex, 
							hierarchy = self.hierarchy, order = self.order, 
							top = self.top, left = self.left, width = self.width, height = self.height, 
							rec_wid = self.width, rec_hei = self.height, 
							error = error_mes, contents = contents)
		
		return VDOM_object.wysiwyg(self, parent, contents=result)

		]]></SourceCode>
