class VDOM_debug(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":
			style = \
				u"""z-index:{zindex}; position:{position}; top:{top}px; left:{left}px;
					width:{width}px; height:{height}px; background:#f0f0f0; font:10px tahoma;
				""".format(
						zindex = self.zindex, position = self.position,
						top = self.top, left = self.left,
						width = self.width, height = self.height)

			innerstyle = u"margin: 5px"

			arguments = request.arguments
			value = u"<b>ARGUMENTS</b><br/>"
			for argument in arguments:
				value += u"<b>{arg}</b>:&nbsp;{arg_value}>".format(
					arg = argument,
					arg_value = arguments.get(argument,[],list))

			id = 'o_' + (self.id).replace('-', '_')

			result = \
				u"""<div id="{id}" style="{style}">
						<div style="{innerstyle}">{val}</div>
					</div>"
				""".format(
						id = id,
						style = style,
						innerstyle = innerstyle,
						val = value.decode("utf-8")
						)
		else:
			result = ""

		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents = ""):
		import utils.wysiwyg

