import StringIO, json, collections
class VDOM_formlist(VDOM_object):	def render(self, parent, contents=""):
		from collections import OrderedDict		display = u"display:none;" if self.visible == "0" else u""		style = u"""position: absolute; z-index: {zind}; top: {top}px; left: {left}px; width: {width}px; height: {height}px;					{display} font: 14px tahoma				""".format(zind = self.zindex, top = self.top, left = self.left, 						width = self.width, height = self.height, display = display)		id = u"o_" + (self.id).replace('-', '_')		
		disabled = u"disabled" if self.disabled == "1" else u""
				select = u"""<select {disabled} id="{id}" name="{name}" tabindex="{tabind}" autocomplete="off" style="{style}">"""\				.format(disabled = disabled, id = id, name = self.name, tabind = self.tabindex, style = style)

		data = {}
		if self.value:		
			try:				data = json.loads(self.value, object_pairs_hook=collections.OrderedDict)
				debug(data)				if isinstance(data, int):
					raise Exception("value: %s. Incorrect format of data" % self.value)
			except:
				raise Exception("value: %s. Incorrect format of data" % self.value)		
		if isinstance(data, dict):
			data = OrderedDict(data)
					res_buffer = StringIO.StringIO(u"")		res_buffer.write(select)		
		selected = u""
		for (index, value) in zip(xrange(len(data)), data):
			if isinstance(data, OrderedDict):
				if value == unicode(self.selectedvalue):
					selected = u"""selected="yes" """
				else:
					selected = u""
				option = u"""<option value="{cur_value}" {selected}>{cur_item}</option>"""\
					.format(cur_value = value, selected = selected, cur_item = data[value])
			else:
				try:
					if index == int(self.selectedvalue):
						selected = u"""selected="yes" """
					else:
						selected = u""
				
				except:
					pass
				option = u"""<option value="{cur_value}" {selected}>{cur_item}</option>"""\
					.format(cur_value = value, selected = selected, cur_item = value)
			res_buffer.write(option)
							res_buffer.write(u"</select>")		result = res_buffer.getvalue()		res_buffer.close()		return VDOM_object.render(self, parent, contents=result)	def wysiwyg(self, parent, contents=""):		result = \			u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}" 						top="{top}" left="{left}" width="{width}" height="{height}">					<svg>						<rect y="{out_top}" x="{out_left}" width="{out_wid}" height="{out_hei}" fill="#FFFFFF" stroke="#000000"/>					</svg>{contents}					<svg>						<rect x="{inn_left}" y="{inn_top}" width="{inn_wid}" height="{inn_hei}" fill="#EEEEEE" stroke="#000000"/>						<polygon fill="black" stroke="#000000" points="{xa},{ya} {xb},{yb} {xc},{yc}"/>					</svg>				</container>			""".format(id = self.id, vis = self.visible, zind = self.zindex, 					hierarchy = self.hierarchy, order = self.order, 					top = self.top, left = self.left, width = self.width, height = self.height,					out_top = 0, out_left = 0, out_wid = int(self.width)-1, out_hei = int(self.height)-1, 					contents = contents,					inn_left = int(self.width)-14, inn_top = 0, inn_wid = 14, inn_hei = int(self.height)-1,					xa = int(self.width)-9, ya = int(self.height) / 2 - 3,					xb = int(self.width)-5, yb = int(self.height) / 2 - 3,					xc = int(self.width)-7, yc = int(self.height) / 2 + 3)				return VDOM_object.wysiwyg(self, parent, contents=result)	]]></SourceCode>
