def on_compile(application, object, action_name, context, objects):
	result=objects
	for xobject in object.get_objects_list():
		result.append({"object": xobject})
	return result
	
class VDOM_mysqlconnector(VDOM_object):	
	def compute(self):
		pass
			
	def render(self, parent, contents=""):
		return ""
		
	def wysiwyg(self, parent, contents=""):		
		result="<container id=\"%s\" visible=\"%s\" hierarchy=\"%s\" >%s</container>"%(self.id, self.visible, self.hierarchy, contents)
		return VDOM_object.wysiwyg(self, parent, contents=result)
		
	def connect(self):
		import MySQLdb
		self.mysql_connection = MySQLdb.connect(host = self.server, user = self.login, passwd = self.password, db = self.database)
		
	def execute_query(self, query):
		import MySQLdb
		if not self.mysql_connection:
			connect()
		cursor = self.mysql_connection.cursor()		
		cursor.execute(query)
		return cursor.fetchall()
		
def connect(app_id, object_id, xml_param):
	import src.xml
	application = src.xml.xml_manager.get_application(app_id)
	object = application.search_object(object_id)
	import MySQLdb

	try:
		cnxn = MySQLdb.connect(host = object.attributes.server, user = object.attributes.login, passwd = object.attributes.password, db = object.attributes.database)
		request.session.value("mysql_connection", cnxn)
		return "<Result>OK. Connected</Result>"
	except Exception, e:
		return "<Error>" + str(e) + "</Error>"
		
def execute_query_xml(app_id, object_id, xml_param):
	try:
		query = ""
		from xml.dom.minidom import parseString
		dom_param = parseString(xml_param)
		query_node = dom_param.getElementsByTagName("query")
		query = str(query_node[0].firstChild.nodeValue)
		import MySQLdb
		cnxn = request.session.value("mysql_connection")
		if not cnxn:
			connect(app_id, object_id, xml_param)
			cnxn = request.session.value("mysql_connection")
		cursor = cnxn.cursor()
		cursor.execute(query)

		result = '<data>'	
		data_rows = cursor.fetchall()
		
		for row in data_rows:
			result += "<row>"
			i = 0
			for val in row:
				result += "<" + str(i) + ">" + str(val) + "</" + str(i) + ">"
				i += 1
			result += "</row>"
		return result + '</data>'
	except Exception, e:
		return "<Error>" + str(e) + "</Error>"
