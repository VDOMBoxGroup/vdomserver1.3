  <SourceCode><![CDATA[def on_compile(application, object, action_name, context, objects):	result=objects	for xobject in object.get_objects_list():		result.append({"object": xobject})	return result	class VDOM_dbschema(VDOM_object):		def compute(self):		database_manager = obsolete_request.database_manager		if not database_manager.check_database(application.id, self.id):			database_manager.create_database(application.id, self.id, self.name)
		else:
			database_manager.rename_database(application.id, self.id, self.name)	def render(self, parent, contents=""):		pass			def wysiwyg(self, parent, contents=""):		result="<container id=\"%s\" visible=\"%s\" hierarchy=\"%s\" >%s</container>"%(self.id, self.visible, self.hierarchy, contents)		return VDOM_object.wysiwyg(self, parent, contents=result)def on_delete(application_id, object_id, param):	obsolete_request.database_manager.delete_database(application_id, object_id)	return ""def execute_query(application_id, object_id, param):	if param:		from database.dbobject import VDOM_sql_query		q = VDOM_sql_query(application_id, object_id, param)		q.commit()		query =q.fetchall()		return (query)	else:		return "Error, query filed: %s"%str(e)def execute_query_xml(application_id, object_id, xml_param):	if xml_param:		from xml.dom.minidom import parseString		dom_param = parseString(xml_param.encode("UTF-8"))		query = dom_param.getElementsByTagName("query_sql")		if query:			from database.dbobject import VDOM_sql_query			q = VDOM_sql_query(application_id, object_id, query[0].firstChild.nodeValue)			q.commit()			query_xml = q.fetchall_xml()			return (query_xml) 	else:		return "<Error>Query failed: %s</Error>"%str(e)				]]></SourceCode>
