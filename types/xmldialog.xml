  <Libraries>
    <Library Target="HTML"><![CDATA[<script type='text/javascript'>

function vdom_xd_load(id,xd){
	xd = $q.trim(xd).replace('<!--[CDATA[', '<![CDATA[').replace(']]-->', ']'+']>');
	var p = $q.parseXML(xd), x = $q(p), w = $q(id), data = [],
		dodiv = function(cn,h,a){
			var vis = parseInt(a.attr('visible') || '0', 10);
			if (vis !== 1) vis = 'style="display:none"';
			return "<div class='item "+cn+"' "+vis+">"+h+"<br style='clear:both' /></div>";
		}
	w.html('');
	x.find("VDOMFormContainer>Components>*").each(function(){
		var t = $q(this), t0 = this, opts, s, sel;

		if (t0.tagName == 'Heading') {
			data.push("<h3>"+t.find('Property[name=text]').text()+"</h3>");
		}
		else if (t0.tagName == 'TextBox') {
			data.push(dodiv('textbox',"<label>"+t.find('Property[name=label]').text()+"</label> <input type='text' name=\""+t.attr('id')+"\" value=\""+t.find('Property[name=defaultValue]').text().replace('"','&quot;')+"\" />", t));
		}
		else if (t0.tagName == 'DropDown') {
			opts = [];
			t.find("Property[name=options]>option").each(function(){
				s = $(this).text().trim();
				if (s !== '') opts.push('<option value="'+$(this).attr('id').replace('"','&quot;')+'">'+s+'</option>');
			});
			data.push(dodiv('dropdown',"<label>"+t.find('Property[name=label]').text()+"</label> <select name=\""+t.attr('id')+"\">"+opts.join(' ')+"</select>", t));
		}
		else if (t0.tagName == 'RadioButton') {
			opts = [], sel = t.attr('selected');
			t.find("Property[name=options]>option").each(function(){
				s = (sel == $(this).attr('id')) ? "checked='checked'" : "";
				opts.push("<label><input type='radio' name=\""+t.attr('id').replace('"','&quot;')+"\" value=\""+$(this).attr('id').replace('"','&quot;')+"\" "+s+" /> "+$(this).text().trim()+"</label>");
			});
			data.push(dodiv('radio',"<label class='title'>"+t.find('Property[name=label]').text()+"</label> "+opts.join(' '), t));
		}
		else if (t0.tagName == 'CheckBox') {
			s = !!t.attr('selected') ? 'checked="checked"' : '';
			data.push(dodiv('checkbox',"<label class='title'>"+t.find('Property[name=label]').text()+"</label> <input type='checkbox' name=\""+t.attr('id')+"\" />", t));
		}
		else if (t0.tagName == 'Upload') {
			data.push(dodiv('upload',"<label class='title'>"+t.find('Property[name=label]').text()+"</label> <input type='file' name=\""+t.attr('id')+"\" />", t));
		}
		else if (t0.tagName == 'TextArea') {
			data.push(dodiv('textarea',"<label class='title'>"+t.find('Property[name=label]').text()+"</label> <textarea name=\""+t.attr('id')+"\">"+t.find('Property[name=defaultValue]').text()+"</textarea>", t));
		}
	});
	w.html("<form action='' method='post' enctype='multipart/form-data' class='xml-dialog-form'>"+data.join(' ')+"<div class='submit'><input type='submit' /></div></form>").dialog('close').dialog('open');
}

</script>]]></Library>
