class VDOM_photogallery(VDOM_object):
	
	def compute(self):
		import utils
		VDOM_object.compute(self)
		res= self.resources
		finalim = res.split(',')
		self.finalimage = []
		if self.prewidth and self.preheight:
			size = self.prewidth,self.preheight
			from utils.imaging import VDOM_imaging
			im = VDOM_imaging()
			for  res_id in finalim:
				res_id = res_id.strip()
				label = "%s_%s_%s" %(res_id,self.prewidth,self.preheight)
				resource = application.resources.get_by_label(self.id, label)
				if resource:
					self.finalimage.append(resource.id)
				else:
					im.load(application.id,res_id)
					im.thumbnail((int(size[0]),int(size[1])))
					self.finalimage.append(im.save_temporary(application.id,self.id,label))
	
	def render(self, contents=""):
		if self.visible == "1":
			if self.rows and self.columns and self.resources:
				gal = ""
				max = len(self.finalimage)
				res = self.finalimage
				res_big = self.resources.split(',')
				j = int(self.skip_images)
				for i in xrange( 1,int(self.rows) +1 ):
					gal += "<tr >"
					for k in xrange( 1,int(self.columns) +1 ):
						if j < max:
							gal += """<td align=center valign=center><a href='/%s.res' class='gal'><img src='/%s.res' border='0' alt='' /></a></td>"""%(res_big[j].strip(),res[j])
							j+=1
						else:
							gal += "<td></td>"
					gal += "</tr>"
				script = "<script language='JavaScript' type='text/javascript'>jQuery('a.gal').flyout(); </script>"
				data = """<table class='glr' width = '%s' height = '%s'> %s </table> """%(self.width,self.height,gal)
				style="position: absolute; z-index: "+self.zindex+"; overflow: auto; top: "+self.top+"px; left: "+self.left+"px; width: "+self.width+"px; height: "+self.height+"px ;"
				id = 'o_' + (self.id).replace('-', '_')
				return """<div id=\"%s\" style=\"%s\">%s</div>%s"""%(id,style,data,script)
			else:
				return  ""
  
	def wysiwyg(self, parent, contents=""):
		w = self.width
		h = self.height
		result = """<container id=\"%s\" zindex=\"%s\" width=\"%s\" height=\"%s\" top=\"%s\" left=\"%s\"  >"""%(self.id, self.zindex,w,h, self.top, self.left)
		result += """<table top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"""%(0,0,w,h)
		if self.rows and self.columns and self.resources:
			gal = ""
			max = len(self.finalimage)
			res = self.finalimage
			j= int(self.skip_images)
			x = y = 0
			w_gal = (int(w))/(int(self.columns))
			h_gal = (int(h))/(int(self.rows))
			res_big = self.resources.split(',')
			for i in xrange( 1,int(self.rows) +1 ):
				result +="<row height=\"%s\">"%h_gal
				for k in xrange( 1,int(self.columns) +1 ):
					if j < max:
						result += "<cell align=\"center\" valign=\"middle\">"\
						"<container width=\"%s\" height=\"%s\">"\
						"<svg >"\
							"<image href=\"#Res(%s)\" x=\"%s\" y=\"%s\" />"\
						"</svg>"\
						"</container>"\
						"</cell>"%(self.prewidth,self.preheight,res[j],0,0)
						j+=1
				result += "</row>"
		result += "</table>"
		result += "</container>"
		return VDOM_object.wysiwyg(self, parent, contents=result)
	]]></SourceCode>
