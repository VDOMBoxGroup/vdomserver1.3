class VDOM_splitter(VDOM_object):

	def render(self, parent, contents=""):

		idn = (self.id).replace('-', '_')
		id = u"o_" + idn

		attributes = self.object.parent.get_attributes()
		if self.direction == "0":
			if "height" in attributes:
				h = int(attributes["height"].value)
			else:
				h = 50
			w = int(self.width)
			l = int(self.left)
			t = 0
		else:
			h = int(self.width)
			if "width" in attributes:
				w = int(attributes["width"].value)
			else:
				w = 50
			l = 0
			t = int(self.top)

		deltamove = int(self.deltamove)
		if deltamove > 0:
			if self.direction == "0":
				dt = t
				dh = h
				dw = deltamove * 2 + w
				dl = l - deltamove
				if dl < 0:
					dw = dw - abs(dl)
					dl = 0
				l = l - dl
				if l < 0: l = 0
			else:
				dw = w
				dh = deltamove * 2 + h
				dt = t - deltamove
				dl = 0
				if dt < 0:
					dh = dh - abs(dt)
					dt = 0
				t = t - dt
				if t < 0: t = 0
		else:
			dl = 0
			dt = 0
			dw = 0
			dh = 0

		style = {
			"background": u"#%s" % self.color,
			"display":    "none" if self.visible == "0" else None,
			"z-index":    self.zindex,
			"position":   self.position,
			"overflow":   "hidden",
			"cursor":     u"%s" % "n-resize" if self.direction == "1" else "e-resize",
			"top":        "%spx" % t,
			"left":       "%spx" % l,
			"width":      "%spx" % w,
			"height":     "%spx" % h }

		html = u"""<div objtype="splitter" objname="%(oname)s" ver="1/2011.09.23" id="%(id)s" style="%(style)s" class="%(classname)s"></div>
		%(contents)s""" % {
			"oname":     self.name,
			"id":        id,
			"style":     "; ".join([u"%s: %s" % (key, value) for key, value in style.iteritems() if value]),
			"classname": self.classname,
			"contents":  contents }

		js = u"""<script type='text/javascript'>
		$j(function($){
			$j("#%(id)s").draggable({
				axis: "%(axis)s", containment: "parent",
				start: function(e,u){
					execEventBinded('%(idn)s', "dragstart", {"X": u.position.left+%(dl)s, "Y": u.position.top+%(dt)s});
				},
				stop: function(e,u){
					execEventBinded('%(idn)s', "dragstop", {"X": u.position.left+%(dl)s, "Y": u.position.top+%(dt)s});
				}
			});
		});
		</script>""" % {
			"axis":   "n-resize" if self.direction == "0" else "e-resize",
			"dt":        dt,
			"dl":        dl,
			"id":        id,
			"idn":       idn }

		if deltamove > 0:
			result = u"""<div style="position:absolute;left:%(left)spx;top:%(top)spx;width:%(width)spx;height:%(height)spx;">%(html)s</div>%(js)s""" % {
				"html":     html,
				"js":       js,
				"top":      "%s" % dt,
				"left":     "%s" % dl,
				"width":    "%s" % dw,
				"height":   "%s" % dh }
		else:
			result = u"%s%s" % (html, js)

		return VDOM_object.render(self, parent, contents=result)


	def wysiwyg(self, parent, contents=""):
		attributes = self.object.parent.get_attributes()
		if self.direction == "0":
			if "height" in attributes:
				h = int(attributes["height"].value)
			else:
				h = 50
			w = int(self.width)
			l = int(self.left)
			t = 0
			m1x = w / 2 - 1
			m1y = h / 2 - 6
			m2x = m1x
			m2y = h / 2
			m3x = m1x
			m3y = h / 2 + 6
		else:
			h = int(self.width)
			if "width" in attributes:
				w = int(attributes["width"].value)
			else:
				w = 50
			l = 0
			t = int(self.top)
			m1x = w / 2 - 6
			m1y = h / 2 - 1
			m2x = w / 2
			m2y = m1y
			m3x = w / 2 + 6
			m3y = m1y
		result = """
			<container id="%(id)s" visible="%(visible)s" zindex="%(zindex)s" hierarchy="%(hierarchy)s" order="%(order)s" top="%(top)s" left="%(left)s" width="%(width)s" height="%(height)s">
				<svg>
					<rect x="0" y="0" width="%(width)s" height="%(height)s" stroke-width="0" fill="#%(color)s" />
					<rect x="%(m1x)s" y="%(m1y)s" width="3" height="3" fill="#%(colorinvert)s" />
					<rect x="%(m2x)s" y="%(m2y)s" width="3" height="3" fill="#%(colorinvert)s" />
					<rect x="%(m3x)s" y="%(m3y)s" width="3" height="3" fill="#%(colorinvert)s" />
				</svg>
			</container>
			""" % {
				"id":          self.id,
				"visible":     self.visible,
				"zindex":      self.zindex,
				"hierarchy":   self.hierarchy,
				"order":       self.order,
				"color":       self.color,
				"colorinvert": "%02x" % (0xffffff ^ int(self.color, 16)),
				"top":         t,
				"m1x":         m1x,
				"m1y":         m1y,
				"m2x":         m2x,
				"m2y":         m2y,
				"m3x":         m3x,
				"m3y":         m3y,
				"left":        l,
				"width":       w,
				"height":      h }

		return VDOM_object.wysiwyg(self, parent, contents=result)


def set_attr(app_id, object_id, param):
	import managers
	o = managers.xml_manager.search_object(app_id, object_id)

	if "left" in param:
		if o.attributes.direction == "1" and int(param["left"]["value"]) > 0:
			o.set_attributes({"left": 0})

	if "top" in param:
		if o.attributes.direction == "0" and int(param["top"]["value"]) > 0:
			o.set_attributes({"top": 0})

	return ""

