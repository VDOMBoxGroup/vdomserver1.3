    <Library Target="Html"><![CDATA[
<script type="text/javascript">
jQuery.fn.accordeon = function() {
	function update(dl) {
		$q("div:not(.active)", dl).css("cursor", "pointer");
		$q("div.active", dl).css("cursor", "");
		$q("div:not(.active) div.content", dl).slideUp();
		$q("div.active div.content", dl).slideDown(300,function(){
			var x = $q(this).parent().attr('id') || '';
			if (x !== '') {
				execEventBinded(x.substring(2, x.length), "itemshow", {name:$q('>div.title',this).text(), index:$q('>div.title',this).index()});
			}
		});
	}
	return this.each(function() {
		var dl = $q(this), active = $q("div.active", dl);
		update(dl);
		$q("div.title", dl).click(function() {
			var p=$q(this).parent();
			if (!p.hasClass("active")) {
				active && active.removeClass("active");
				active = p.addClass("active");
				update(dl);
			}
		});
	});
};
function vdom_accordeon(id,opened) {
	var i=$q(id), o = opened || false;
	$q('>div:visible',i).each(function(r){
		$q(this).attr('rel',r+1).css({display:'block',position:'relative',left:0,top:0,height:'auto',overflow:'hidden'});
		var mh=0;
		$q('div>*',this).each(function(){
			mht = $q(this).height()+$q(this).position().top;
			if (mht>mh) mh = mht;
		});
		$q('div.content',this).height(mh).css({display:'block',position:'relative'});
	});
	if (o !== false) $q('>div[rel="%(opened)s"]',i).addClass('active');
	i.accordeon();
}
</script>
]]></Library>
