def on_compile(application_object, object, action_name, context, objects):
	result = objects
	attributes = object.get_attributes()
	source_object_id = attributes["source_object"].value
	if source_object_id:
		source_object = application.objects.search(source_object_id)
		
		if not source_object:
			return result
		
		if source_object.type.dynamic:
			object.dynamic[(action_name, context)] # object.dynamic=1
		object.optimization_priority = source_object.type.optimization_priority
		
		xobject = {"object": source_object,
			"render":  ["if self.top!='':",
						"\tself.%s.top=self.top" % source_object.name,
						"if self.left!='':",
						"\tself.%s.left=self.left" % source_object.name],
			"wysiwyg": ["self.%s.top=0" % source_object.name,
						"self.%s.left=0" % source_object.name]}
		
		result.append(xobject)
	return result

class VDOM_copy(VDOM_object):

	def __init__(self, id):
		VDOM_object.__init__(self, id)


	def check_cache(self):
		if self.source_object_cache != self.source_object:
			#Reset copy position
			self.source_object_cache = self.source_object
			self.top = ""
			self.left = ""
			self.update("source_object_cache", "top", "left")


	def render(self, parent, contents=""):
		self.check_cache()
		if self.visible == "1":
			result = contents
		else:
			result = ""
		return VDOM_object.render(self, parent, contents=result)


	def wysiwyg(self, parent, contents=""):
		self.check_cache()

		if self.source_object:
			object = application.objects.search(self.source_object)
	
			if not object:
				width = 100
				height = 100
				result = u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}" 
								top="{top}" left="{left}" width="{width}" height="{height}" contents="static">
								<svg>
									<rect x="0" y="0" width="{width}" height="{height}" fill="#{designcolor}"/>
								</svg>
								<text top="0" left="0" width="{width}" height="{height}" color="#{designcolorinvert}">Empty copy</text>
								{contents}
							</container>
						""".format(id = self.id, vis = self.visible, zind = self.zindex, 
								hierarchy = self.hierarchy, order = self.order, 
								designcolor = self.designcolor, 
								designcolorinvert = "%02x" % (0xffffff ^ int(self.designcolor, 16)),
								top = self.top, left = self.left, width = width, height = height, contents = contents)
				return VDOM_object.wysiwyg(self, parent, contents=result)
	
			if self.top:
				top = self.top
			else: 
				top = object.get_attributes()["top"].value
			
			if self.left:
				left = self.left
			else:
				left = object.get_attributes()["left"].value
			
			width = object.get_attributes()["width"].value

			if "height" in object.get_attributes():
				height = u"""height="%s" """ % object.get_attributes()["height"].value
				height2 = u"""height="%s" """ % str(int(object.get_attributes()["height"].value) + 2)
			else:
				height = u""
				height = u""

			if "zindex" in object.get_attributes():
				zindex = object.get_attributes()["zindex"].value
			else:
				zindex = self.zindex

			result = u"""<container id="{id}" visible="{vis}" zindex="{zind}" 
							top="{top}" left="{left}" width="{width2}" {height2} contents="static">
							<svg>
								<rect x="0" y="0" width="{width2}" {height2} fill="#{designcolor}"/>
							</svg>
							<container top="1" left="1" width="{width}" {height} contents="static">
								{contents}
							</container>
						</container>
					""".format(id = self.id, vis = self.visible, zind = zindex, 
							designcolor = self.designcolor, 
							width2 = str(int(width) + 2), height2 = height2,
							top = top, left = left, width = width, height = height, contents = contents)

		else:
			top = self.top if self.top else 10
			left = self.left if self.left else 10
			width = 100
			height = 100
			result = u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}" 
								top="{top}" left="{left}" width="{width}" height="{height}" 
								backgroundcolor="#EEEEEE" contents="static">
							<svg>
								<rect x="0" y="0" width="{width}" height="{height}" fill="#{designcolor}"/>
							</svg>
							<text top="0" left="0" width="{width}" height="{height}" color="#{designcolorinvert}">Empty copy</text>
							{contents}
						</container>
					""".format(id = self.id, vis = self.visible, zind = self.zindex, 
							hierarchy = self.hierarchy, order = self.order, 
							designcolor = self.designcolor, 
							designcolorinvert = "%02x" % (0xffffff ^ int(self.designcolor, 16)),
							top = top, left = left, width = width, height = height, contents = contents)

		return VDOM_object.wysiwyg(self, parent, contents=result)

def set_attr(app_id, object_id, param):
	if "source_object" in param:
		src_old = param["source_object"]["old_value"]
		src_new = param["source_object"]["value"]
		obj_old = application.objects.search(src_old)	# try to find old and new source objects
		obj_new = application.objects.search(src_new)
		if obj_old:
			obj_old.copy_objects.remove(object_id)
		if obj_new:
			obj_new.copy_objects.append(object_id)
	return ""

