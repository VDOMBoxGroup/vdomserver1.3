import src.xml

def on_compile(application, object, action_name, context, objects):
	result=objects
	manager=src.xml.xml_manager
	attributes=object.get_attributes()
	source_object_id=attributes["source_object"].value
	#debug("[Copy] Source object %s"%source_object_id)
	if source_object_id:
		source_object=manager.search_object(application.id, source_object_id)
		if not source_object:
			return result
		if source_object.type.dynamic:
			object.dynamic[(action_name, context)] # object.dynamic=1
		object.optimization_priority=source_object.type.optimization_priority
		if object.dynamic[(action_name, context)]:
			debug("[Copy] Assume %s as dynamic"%(object.id))
		else:
			debug("[Copy] Assume %s as static"%(object.id))
		xobject={"object": source_object,
		"render": ["if self.top!='':",
			"\tself.%s.top=self.top"%source_object.name,
			"if self.left!='':",
			"\tself.%s.left=self.left"%source_object.name],
		"wysiwyg": ["self.%s.top=0"%source_object.name,
			"self.%s.left=0"%source_object.name]}
		result.append(xobject)
	return result

class VDOM_copy(VDOM_object):

	def __init__(self, id):
		VDOM_object.__init__(self, id)

	def check_cache(self):
		if self.source_object_cache!=self.source_object:
			debug("[Copy] Reset copy position")
			self.source_object_cache=self.source_object
			self.top=""
			self.left=""
			self.update("source_object_cache", "top", "left")

	def render(self, parent, contents=""):
		self.check_cache()
		if self.visible == "1":
			result=contents
		else:
			result=""
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		self.check_cache()
		result = ""
		if self.source_object:
			object=request.vdom.search_object(self.source_object)
			if not object:
				result="""<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\" contents=\"static\"><text top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\" color=\"#FFFFFF\">%s</text>%s</container>"""%(self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, 100, 100, 0, 0, 100, 100, "Empty copy", contents)
				return VDOM_object.wysiwyg(self, parent, contents=result)
			if self.top:
				top=self.top
			else:
				top=object.get_attributes()["top"].value
			if self.left:
				left=self.left
			else:
				left=object.get_attributes()["left"].value
			width=object.get_attributes()["width"].value
			height_code = ""
			if "height" in object.get_attributes():
				height_code="height=\""+object.get_attributes()["height"].value+"\""
			zindex = self.zindex
			if "zindex" in object.get_attributes():
				zindex=object.get_attributes()["zindex"].value
			result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" %s contents=\"static\">%s</container>"%(self.id, self.visible, zindex, top, left, width, height_code, contents)
		else:
			label="Empty copy"
			if self.top:
				top=self.top
			else:
				top=10
			if self.left:
				left=self.left
			else:
				left=10
			width=100
			height=100
			result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\" backgroundcolor=\"#EEEEEE\" contents=\"static\">"\
					"<text top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\" color=\"#FFFFFF\">%s</text>"\
					"%s</container>"%(self.id, self.visible, self.zindex, self.hierarchy, self.order, top, left, width, height, 
					0, 0, width, height, label, contents)
		return VDOM_object.wysiwyg(self, parent, contents=result)

def set_attr(app_id, object_id, param):
	if "source_object" in param:
		src_old = param["source_object"]["old_value"]
		src_new = param["source_object"]["value"]
		import src.xml
		obj_old = src.xml.xml_manager.search_object(app_id, src_old)	# try to find old and new source objects
		obj_new = src.xml.xml_manager.search_object(app_id, src_new)
		if obj_old:
			obj_old.copy_objects.remove(object_id)
		if obj_new:
			obj_new.copy_objects.append(object_id)
	return ""

