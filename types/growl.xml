  <SourceCode><![CDATA[

import re

class VDOM_growl(VDOM_object):

	def render(self, parent, contents=""):

		vstyle = u''
		style = int(self.style)
		if style > 0:
			x = style
			if x > 5: x = 5
			if x > 1:
				xa = {
					1: 'yellow information',
					2: 'yellow warning',
					3: 'red error'
				}
				vstyle = xa[x]
		else:
			if self.userclass != "":
				vstyle = unicode(self.userclass).replace('"', '&quot;')

		res = u''
		if self.active == '1':
			title = unicode(self.title).strip().replace('"', '&quot;')
			text = unicode(self.text).strip().replace('"', '&quot;')
			if title != '' and text != '':
				#res += "<span class='growls' style='display:none'><span>%s</span><div>%s</div></span>"%(self.title, self.text)
				res = u"""<script type="text/javascript">
jQuery(function($){
	$.gritter.add({title:"%(title)s",text:"%(text)s",sticky:false,time:'',class_name:"%(vstyle)s"});
});
</script>""" % { "vstyle": vstyle, "title": title , "text": text }

		return res

	def wysiwyg(self, parent, contents=""):
		result = \
			u"<container id=\"%s\" hierarchy=\"%s\" top=\"%s\" left=\"%s\" width=\"90\">"\
				"<svg>"\
					"<rect opacity=\"0.5\" x=\"0\" y=\"0\" width=\"80\" height=\"50\" fill=\"#D8C760\" stroke=\"#B1A44F\"/>"\
				"</svg>"\
				"<text top=\"2\" left=\"5\" width=\"80\" color=\"#67602E\">Growl msg</text>"\
			"</container>" % (
				self.id, self.hierarchy, self.top, self.left)
		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]></SourceCode>
