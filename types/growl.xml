import re
	
class VDOM_growl(VDOM_object):

	def render(self, parent, contents=""):
		z = ''
		style = int(self.style)
		if style > 0:
			x = style
			if style > 5: x = 5
			if x > 1:
				xa = [ '', 'red', 'green', 'blue', 'black', 'white' ]
				z = xa[x]
		else:
			if self.userclass != "":
				z = unicode(self.userclass)
		vstyle = ',"class_name":"%s"' % z
		res = """<script type="text/javascript">
jQuery(document).ready(function($){
	if (typeof $.growl === 'undefined') {
		$.growl=function(t,m){
			$.gritter.add({'title':t,'text':m,'sticky':false,'time':''%(class)s});
		}
	}
	$('span.growls').each(function(){
		var t=$.trim($('>span',this).html()), m=$.trim($('>div',this).html());
		if (t!=''&&m!='') $.growl(t,m);
	});
});
</script>""" % { "class": vstyle }
		if self.active == '1':
			if self.title.strip() != '' and self.text.strip() != '':
				res += "<span class='growls' style='display:none'><span>%s</span><div>%s</div></span>"%(self.title, self.text)
		else:
			res += ""

		return res

	def wysiwyg(self, parent, contents=""):
		result="<container id=\"%s\" hierarchy=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\">"\
				"<svg>"\
					"<rect opacity=\"0.5\" x=\"0\" y=\"0\" width=\"80\" height=\"50\" fill=\"#D8C760\" stroke=\"#B1A44F\"/>"\
				"</svg>"\
				"<text top=\"2\" left=\"5\" width=\"80\" color=\"#67602E\">Growl msg</text>"\
				"</container>"%(
					self.id, self.hierarchy, self.top, self.left, self.width)
		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]></SourceCode>
