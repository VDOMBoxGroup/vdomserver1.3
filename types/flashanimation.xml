class VDOM_flashanimation(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":
			style="z-index: %s;position: %s;top: %spx;left: %spx;width: %spx;height: %spx"%(self.zindex, self.position, self.top, self.left, self.width, self.height)
			qua = "high"
			if "1" == self.quality:
				qua = "medium"
			elif "2" == self.quality:
				qua = "low"
			data = """<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"
			 codebase="http://fpdownload.macromedia.com/get/flashplayer/current/swflash.cab" width="%s" height="%s">
				<param name="movie" value="/%s.res">
				<param name="quality" value="%s">
				<param name="FlashVars" value="%s" />
				<param name="wmode" value="transparent">
				<embed src="/%s.res" quality="%s" wmode="transparent"
				pluginspage="http://www.adobe.com/go/getflashplayer" type="application/x-shockwave-flash" width="%s" height="%s"
				FlashVars="%s">
				</embed>
				</object>""" % (self.width, self.height, self.file, qua, self.flashvars, self.file, qua, self.width, self.height, self.flashvars)
			id = 'o_' + (self.id).replace('-', '_')
			result="<div id=\"%s\" style=\"%s\">%s%s</div>"%(id, style, data, contents)
		else:
			result=""
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		import utils.wysiwyg

		image_id = "74a93184-b726-4325-8f46-6eceb2309878"
		image_width = image_height = 58

		outer_width = int(self.width)
		outer_height = int(self.height)

		image_x = image_y = 0
		image_x, image_y, image_width, image_height = utils.wysiwyg.get_centered_image_metrics( image_width, image_height, outer_width, outer_height)

		resource_name_text = ""
		if self.file:
			res = application.resources.get(self.file)
			resource_name = res.name if res else ""
			resource_name_text = \
				u"""<text x="0" y="{res_name_y}" width="{width}" align="center" fill="#aaaaaa" font-size="14">{resource_name}</text>""".format(
					width = outer_width,
					resource_name = resource_name,
					res_name_y = image_y + image_height - 20
				)


		result = \
			u"""<container id="{id}" visible="{visible}" zindex="{zindex}" hierarchy="{hierarchy}" order="{order}" top="{top}" left="{left}" width="{outer_width}" height="{outer_height}">
					<svg>
						<image x="{image_x}" y="{image_y}" href="#Res({image_id})" width="{image_width}" height="{image_height}"/>
						{resource_name_text}
					</svg>
					{contents}
				</container>
				""".format(
					id = self.id,
					visible = self.visible,
					zindex = self.zindex,
					hierarchy = self.hierarchy,
					order = self.order,
					top = self.top,
					left = self.left,
					outer_width = outer_width,
					outer_height = outer_height,
					image_x = image_x,
					image_y = image_y,
					image_id = image_id,
					image_width = image_width,
					image_height = image_height,
					contents = contents,
					resource_name_text = resource_name_text
				)

		return VDOM_object.wysiwyg(self, parent, contents=result)

]]></SourceCode>
