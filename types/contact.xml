class VDOM_contact(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":

			id = u"o_" + (self.id).replace('-', '_')

			args = request.arguments
			if "subject" in args and "message" in args and "fromaddr" in args:
				subj = args["subject"]
				mess = args["message"]
				froma = args["fromaddr"]
				if len(mess) > 0 or len(subj) > 0:
					server.mailer.send(froma, self.email, subj, mess)

			if self.overflow == "1":
				overflow = u"hidden"
			elif self.overflow == "2":
				overflow = u"scroll"
			elif self.overflow == "3":
				overflow = u"visible"
			else:
				overflow = u"auto"

			#style = u"font-size:14px; font-family: Tahoma; z-index: %s;position: %s;top: %spx;left: %spx;width: %spx;height: %spx;background: #EEEEEE" % (
			#	self.zindex, self.position, self.top, self.left, self.width, self.height)
			style = u"z-index: %s; position: absolute; top: %spx; left: %spx; width: %spx; height: %spx; overflow: %s;" % (
				self.zindex, self.top, self.left, self.width, self.height, overflow)
			#table_style = u"font-size:14px; font-family: Tahoma;"

			if self.language == "0":
				addr = u"Your address: "
				subj = u"Subject: "
				msg  = u"Message: "
				send = u"Send"
			elif self.language == "1":
				addr = u"Ваш e-mail: "
				subj = u"Тема: "
				msg  = u"Сообщение: "
				send = u"Отправить"
			else:
				addr = u"Votre e-mail: "
				subj = u"Sujet: "
				msg  = u"Message: "
				send = u"Envoyer "

			css = u"""
				<style>
				#%(id)s label,
				#%(id)s input,
				#%(id)s textarea {
					display: block;
				}
				#%(id)s label {
					float: left;
					white-space: nowrap;
				}
				#%(id)s input {
					width: %(iw)s; /* width: %(w)s */
				}
				#%(id)s input,
				#%(id)s textarea {
					float: right;
				}
				#%(id)s textarea {
					width: 100%%;
				}
				#%(id)s .send {
					width: auto;
					float: none;
				}
				</style>
				""" % {
						"id": id,
						"w": self.width,
						"iw": "100%" if (int(self.width) <= 200) else "50%"
					}

			form_data = u"""
				<label for="addr_{id}">{addr}</label>
				<input class="addr" type="text" name="fromaddr" />
				<div style='clear:both'></div>
				<label for="subj_{id}">{subj}</label>
				<input class="subj" type="text" name="subject" />
				<div style='clear:both'></div>
				<label for="msg_{id}">{msg}</label>
				<textarea class="msg" name="message" rows="6" cols="30"></textarea>
				<div style='clear:both'></div>
				<input class="send" type="submit" value="{send}" />
				""".format(
						id   = id,
						addr = addr,
						subj = subj,
						msg  = msg,
						send = send.replace('"', '&quot;') )

			p = application.objects.search(self.id)
			while p.parent:
				p = p.parent
			ref_obj = application.objects.search(p.id)
			target = "/%s.vdom" % ref_obj.name if ref_obj else ""

			classname = u"""class="%s" """ % self.classname if self.classname else u""

			if VDOM_CONFIG_1["DEBUG"] == "1":
				debug_info = u"objtype='contact' objname='%s' ver='%s'" % (self.name, self.type.version)
			else:
				debug_info = u""

			result = u"""{css}<div {debug_info} id="{id}" {classname}><form name="{name}" style="{form_style}" method=post action="{target}">{form}</form></div>""".format(
				css        = css,
				debug_info = debug_info,
				classname  = classname,
				id         = id,
				name       = self.name,
				form_style = style,
				target     = target,
				form       = form_data )
		else:
			result = ""

		return VDOM_object.render(self, parent, contents=result)


	def wysiwyg(self, parent, contents=""):

		if self.language == "0":
			addr = "Your address :"
			subj = "Subject :"
			msg = "Message :"
			send = "Send"
		elif self.language == "1":
			addr = "Ваш e-mail :".decode("utf8")
			subj = "Тема :".decode("utf8")
			msg = "Сообщение :".decode("utf8")
			send = "Отправить".decode("utf8")
		else:
			addr = "Votre e-mail :".decode("utf8")
			subj = "Sujet :".decode("utf8")
			msg = "Message :".decode("utf8")
			send = "Envoyer".decode("utf8")

		if int(self.width) < 70:
			result = "<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
						"<svg>"\
							"<rect width=\"%s\" height=\"%s\" fill=\"#EEEEEE\"/>"\
						"</svg>"\
					"</container>"%(
						self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, self.height,
						int(self.width), int(self.height))
		elif int(self.width) < 85:
			result = "<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
						"<svg>"\
							"<rect width=\"%s\" height=\"%s\" fill=\"#EEEEEE\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#CCCCCC\" stroke=\"#000000\"/>"\
						"</svg>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
