						"</svg>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" height=\"%s\" color=\"#000000\" textalign=\"center\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
					"%s</container>"%(
						self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, self.height,
						int(self.width), int(self.height),
						3, 90, 150, 20,
						28, 90, 150, 20,
						70, 0, int(self.width) - 1, int(self.height)- 100,
						int(self.height)-23, 70, 22,
						3, int(self.width) - 1, "Tahoma", 14, addr,
						28, int(self.width) - 1 , "Tahoma", 14, subj,
						50, int(self.width) - 1, "Tahoma", 14, msg,
						int(self.height) - 20, 70, 18, "Tahoma", 12, send,
						contents)

		return VDOM_object.wysiwyg(self, parent, contents=result)


def set_attr(app_id, object_id, param):
	o = application.objects.search(object_id)

	if "width" in param:
		width = param["width"]["value"]
		if int(width) < 70:
			o.set_attributes({"width": "70"})

