from utils.csrf import create_csrf_token, csrf_token_arg_name

class VDOM_contact(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":

			id = u"o_" + (self.id).replace('-', '_')

			args = request.arguments
			if "subject" in args and "message" in args and "fromaddr" in args:
				subj = args["subject"]
				mess = args["message"]
				froma = args["fromaddr"]
				if len(mess) > 0 or len(subj) > 0:
					server.mailer.send(froma, self.email, subj, mess)

			if self.overflow == "1":
				overflow = u"hidden"
			elif self.overflow == "2":
				overflow = u"scroll"
			elif self.overflow == "3":
				overflow = u"visible"
			else:
				overflow = u"auto"

			style = u"z-index: %s; position: absolute; top: %spx; left: %spx; width: %spx; overflow: %s;" % (
				self.zindex, self.top, self.left, self.width, overflow)

			if self.language == "0":
				addr = u"Your address: "
				subj = u"Subject: "
				msg  = u"Message: "
				send = u"Send"
			elif self.language == "1":
				addr = u"Ваш e-mail: "
				subj = u"Тема: "
				msg  = u"Сообщение: "
				send = u"Отправить"
			else:
				addr = u"Votre e-mail: "
				subj = u"Sujet: "
				msg  = u"Message: "
				send = u"Envoyer "

			css = u"""
				<style>
				#%(id)s label,
				#%(id)s input,
				#%(id)s textarea {
					display: block;
				}
				#%(id)s label {
					float: left;
					white-space: nowrap;
				}
				#%(id)s input {
					width: %(iw)s; /* width: %(w)s */
				}
				#%(id)s input,
				#%(id)s textarea {
					float: right;
				}
				#%(id)s textarea {
					width: 100%%;
				}
				#%(id)s .send {
					width: auto;
					float: none;
				}
				</style>
				""" % {
						"id": id,
						"w": self.width,
						"iw": "100%" if (int(self.width) <= 200) else "50%"
					}

			csrftoken = '<input type="hidden" name="%s" value="%s">' % (csrf_token_arg_name, create_csrf_token())

			form_data = u"""
				<label for="addr_{id}">{addr}</label>
				<input class="addr" type="text" name="fromaddr" />
				<div style='clear:both'></div>
				<label for="subj_{id}">{subj}</label>
				<input class="subj" type="text" name="subject" />
				<div style='clear:both'></div>
				<label for="msg_{id}">{msg}</label>
				<textarea class="msg" name="message" rows="6" cols="30"></textarea>
				<div style='clear:both'></div>
				<input class="send" type="submit" value="{send}" />
				{csrftoken}
				""".format(
						id   = id,
						addr = addr,
						subj = subj,
						msg  = msg,
						send = send.replace('"', '&quot;'),
						csrftoken = csrftoken )

			p = application.objects.search(self.id)
			while p.parent:
				p = p.parent
			ref_obj = application.objects.search(p.id)
			target = "/%s.vdom" % ref_obj.name if ref_obj else ""

			classname = u"""class="%s" """ % self.classname if self.classname else u""

			if VDOM_CONFIG_1["DEBUG"] == "1":
				debug_info = u"objtype='contact' objname='%s' ver='%s'" % (self.name, self.type.version)
			else:
				debug_info = u""

			result = u"""{css}<div {debug_info} id="{id}" {classname}><form name="{name}" style="{form_style}" method=post action="{target}">{form}</form></div>""".format(
				css        = css,
				debug_info = debug_info,
				classname  = classname,
				id         = id,
				name       = self.name,
				form_style = style,
				target     = target,
				form       = form_data )
		else:
			result = ""

		return VDOM_object.render(self, parent, contents=result)


	def wysiwyg(self, parent, contents=""):
		if self.overflow == "1":
			overflow = u"hidden"
		elif self.overflow == "2":
			overflow = u"scroll"
		elif self.overflow == "3":
			overflow = u"visible"
		else:
			overflow = u"auto"
		
		style = u"position: absolute; width: {width}px; overflow: {overflow}; font-family: verdana; font-size: 12".format(
			width = self.width, overflow = overflow)

		if self.language == "0":	# English
			addr = u"Your address: "
			subj = u"Subject: "
			msg  = u"Message: "
			send = u"Send"
		elif self.language == "1":	# Russian
			addr = u"Ваш e-mail: "
			subj = u"Тема: "
			msg  = u"Сообщение: "
			send = u"Отправить"
		else:						# French
			addr = u"Votre e-mail: " 
			subj = u"Sujet: "
			msg  = u"Message: "
			send = u"Envoyer "
		
		form_data = \
			u"""
			<label style="display: block; float: left; white-space: nowrap;">{addr}</label>
			<input type="text" name="fromaddr" style="display: block; width:{input_width}; float: right;" />
			<div style='clear:both'></div>
			<label style="display: block; float: left; white-space: nowrap;">{subj}</label>
			<input type="text" name="subject" style="display: block; width:{input_width}; float: right;" />
			<div style='clear:both'></div>
			<label style="display: block; float: left; white-space: nowrap;">{msg}</label>
			<textarea name="message" rows="6" cols="30" style="display: block; float: right; width: 100%; ">&nbsp;</textarea>
			<input class="send" type="submit" value="{send}" style="width:auto; float: none; clear:both; display: block;" />
			""".format(
					addr = addr,
					subj = subj,
					msg  = msg,
					input_width = "100%" if (int(self.width) <= 200) else "50%",
					send = send.replace('"', '&quot;') )

		html_content = u"""<div ><form name="{name}" style="{form_style}">{form}</form></div>""".format(
			name       = self.name,
			form_style = style,
			form       = form_data )
		
		result = \
			u"""<container id="{id}" zindex="{zindex}" width="{width}" top="{top}" left="{left}">
					<htmltext width="{width}" blendMode="normal" locked="true" overflow="visible" >
						{html_content}
					</htmltext>
				</container>
			""".format(id = self.id, zindex = self.zindex, width=self.width, height=self.height,
				top = self.top, left = self.left,
				html_content = html_content)
		
		return VDOM_object.wysiwyg(self, parent, contents=result)


def set_attr(app_id, object_id, param):
	o = application.objects.search(object_id)

	if "width" in param:
		width = param["width"]["value"]
		if int(width) < 70:
			o.set_attributes({"width": "70"})

