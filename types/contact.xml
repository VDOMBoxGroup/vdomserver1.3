						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" height=\"%s\" color=\"#000000\" textalign=\"center\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
					"%s</container>"%(
						self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, self.height,
						int(self.width), int(self.height),
						33, int(self.width) - 1, 20,
						75, int(self.width) - 1, 20,
						115, int(self.width) - 1, int(self.height)- 145,
						int(self.height)-22, 70, 22,
						0, int(self.width) - 1, "Tahoma", 14, "Your",
						17, int(self.width) - 1, "Tahoma", 14, "address: ",
						59, int(self.width) - 1, "Tahoma", 14, subj,
						99, int(self.width) -1, "Tahoma", 14, msg,
						int(self.height)-18, 70, 18, "Tahoma", 12, send,
						contents)
		elif int(self.width) < 235:
			result = "<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
						"<svg>"\
							"<rect width=\"%s\" height=\"%s\" fill=\"#EEEEEE\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#CCCCCC\" stroke=\"#000000\"/>"\
						"</svg>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" color=\"#000000\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
						"<text top=\"%s\" width=\"%s\" height=\"%s\" color=\"#000000\" textalign=\"center\" fontfamily=\"%s\" fontsize=\"%s\">%s</text>"\
					"%s</container>"%(
						self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, self.height,
						int(self.width), int(self.height),
						19, int(self.width) - 1, 20,
						61, int(self.width) - 1, 20,
						101, int(self.width) - 1, int(self.height)- 130,
						int(self.height) - 23, 70, 22,
						0, self.width, "Tahoma", 14, addr,
						42, int(self.width) - 1, "Tahoma", 14, subj,
						82, int(self.width) - 1, "Tahoma", 14, msg,
						int(self.height)-21, 70, 18, "Tahoma", 12, send,
						contents)
		else:
			result = "<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
						"<svg>"\
							"<rect width=\"%s\" height=\"%s\" fill=\"#EEEEEE\"/>"\
							"<rect y=\"%s\" x=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" x=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
							"<rect y=\"%s\" x=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
