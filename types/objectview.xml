import e2vdom
import managers
import thread

def create_dupes_recurse(application, res, object, children, aa, w):
	result = res
	for xobject in children:

		# create new object
		obj_t = application.objects.create(xobject.type.id, object.id)
		obj_id = obj_t.id #obj_t[1]

		# add to list of created objects for delete later
		aa.append(obj_id)

		# get it
		obj = application.objects.search(obj_id)

		# copy object name if this is not 1st child container in objectviewtemplate
		#if not obj.parent.type.container == 2:
		#if not obj.parent.parent:
		#	obj.set_name(xobject.name)
		try:
			obj.set_name(xobject.name)
		except:
			pass

		# copy actions
		obj.actions = xobject.actions

		# copy properties
		#for name, val in xobject.get_attributes().iteritems():
		#	obj.set_attribute_ex(name, val, False)
		obj_a = xobject.get_attributes()
		for a in obj_a:
			#print '-------'
			#print dir(obj_a[a])
			obj.set_attribute_ex(obj_a[a].name, obj_a[a].value, False)

		#result.append({
		#	"object": obj
		#})

		if xobject.type.container == 2:
			result, aa = create_dupes_recurse(application, result, obj, xobject.get_objects_list(), aa, w)

	return result, aa

def on_compile(application_object, object, action_name, context, objects):
	debug("[on_compile] ####### begin")
	result = objects
	attributes = object.get_attributes()
	pattern = attributes["pattern"].value
	if pattern:
		source_object = application.objects.search(pattern)

		if not source_object:
			return result

		if source_object.type.dynamic:
			object.dynamic[(action_name, context)] # object.dynamic=1
		object.optimization_priority = source_object.type.optimization_priority

		aa = []

		#res, aa = create_dupes_recurse(application, result, object, source_object.get_objects_list(), aa)
		#result.append({
		#	"object": res
		#})

		#res2, aa = create_dupes_recurse(application, result, object, source_object.get_objects_list(), aa)
		#result.append({
		#	"object": res2
		#	,"render": [u"""%s.top="0" """ % res2.name, u"""%s.left="300" """ % res2.name]
		#})

		debug("[ov] ####### get ov data & json it ")
		od_t = u"""%s""" % (attributes["data"].value).replace("\n", "")
		print od_t
		import json
		try:
			od = json.loads(od_t)
		except:
			od = []
		debug("[ov] ####### jsoned: ")
		print json.dumps(od)

		sobj_attributes = source_object.get_attributes()

		#nn = 1
		nn = 0
		obja = []
		for w in od:
			# create new object for item container
			obj_t = application.objects.create('0bb24c16-14fe-08ef-858f-ba24aafc8049', object.id)
			obj_id = obj_t.id
			aa.append(obj_id)
			obj = application.objects.search(obj_id)
			#obja.append(application.objects.search(obj_id))

			obj.attributes = source_object.attributes
			obj.set_attribute_ex('data', w, False)

			obj.actions = source_object.actions

			res, aa = create_dupes_recurse(application, result, obj, source_object.get_objects_list(), aa, w)

			result.append({
				"object": obj #res
				#,"render": render
				#,"render": [u"self.%s.data=\"\"\"%s\"\"\" " % (res.name, z)]
				,"render": [
					u"self.%s.visible=\"1\"" % (obj.name)
					,u"self.%s.width=\"%s\"" % (obj.name, sobj_attributes['width'].value)
					,u"self.%s.height=\"%s\"" % (obj.name, sobj_attributes['height'].value)
					#,u"self.%s.data=\"\"\"%s\"\"\"" % (obj.name, w)
					,u"self.%(name)s.classname+=\" %(class)s-item %(class)s-item-%(nn)s\"" % {
						"name":  obj.name, 
						"class": attributes['classname'].value,
						"nn":    str(nn)
					}
				]
			})

			nn += 1

		debug("[ov] ####### store ov_created_objects ")
		setattr(managers.request_manager.current, "ov_created_objects_" + str(thread.get_ident()), aa)


	debug("[on_compile] ####### end")
	return result

class VDOM_objectview(VDOM_object):

	def __init__(self, id):
		VDOM_object.__init__(self, id)

	def render(self, parent, contents = ""):

		# remove on_compile's objects 

		debug("[ov] ####### invalidate source cache")
		managers.source_cache.invalidate(application.id, self.id)

		debug("[ov] ####### get ov_created_objects")
		aa = getattr(managers.request_manager.current, "ov_created_objects_" + str(thread.get_ident()), [])

		#debug("[ov] ####### delete ov_created_objects")
		for oi in aa:
			#debug("[ov] ####### ... find " + oi)
			x = application.objects.search(oi)
			if x:
			#	debug("[ov] ####### ... kill them")
				application.objects.delete(x.id)
			#else:
			#	debug("[ov] ####### ... none")


		# render

		display = u" display:none; " if self.visible == "0" else u""

		e2vdom.process(self, parent)

		if self.overflow == "1":
			overflow = u"hidden"
		elif self.overflow == "2":
			overflow = u"scroll"
		elif self.overflow == "3":
			overflow = u"visible"
		else:
			overflow = u"auto"

		height = u"height: %spx;" % self.height

		style = u"""{display} z-index: {zind}; position: {pos}; top: {top}px; left: {left}px;
					width: {width}px; overflow: {overflow}; {height}"""\
				.format(display = display, zind = self.zindex, pos = self.position,
						top = self.top, left = self.left, width = self.width, overflow = overflow, height = height)

		id = u"o_" + (self.id).replace('-', '_')

		classname = u"""class="%s" """ % self.classname if self.classname else u""

		result = u"""<div id="{id}" style="{style}" {classname}>{contents}</div>""".format(
			id = id, style = style, classname = classname, contents = contents
		)

		return VDOM_object.render(self, parent, contents=result)


	def wysiwyg(self, parent, contents=""):

		# remove on_compile's objects 
		managers.source_cache.invalidate(application.id, self.id)
		aa = getattr(managers.request_manager.current, "ov_created_objects_" + str(thread.get_ident()), [])
		for oi in aa:
			x = application.objects.search(oi)
			if x:
				application.objects.delete(x.id)

		
		html_items = u""
		datalen = 0
		if self.data != "":
			od_t = u"""%s""" % (self.data).replace("\n", "")
			import json
			try:
				od = json.loads(od_t)
			except:
				od = []
			if len(od) > 0:
				datalen = len(od)
				nn = 0
				for w in od:
					nn += 1
					b, tail = w[:0], w
					while len(tail)>2:
						b += tail[:2] + "&shy;"
						tail = tail[2:]
					data = b + tail
					html_items += u"<div><b>{nn}</b> {data}</div>".format(
						nn = nn, data = data
					)

		html = u"""
		<style type="text/css">
		.ov_container {
		postition: relative;
		}
			.ov_container div {
			float: left;
			width: 100px;
			height: 55px;
			overflow: hidden;
			padding: 1px;
			margin: 0 1px 1px 0;
			border: 1px solid #ccc;
			background: #ddd;
			white-space: wrap;
			color: #555;
			font-size: 11px;
			}
		</style>
		<div class="ov_container">%(items)s</div>
		""" % { "items": html_items }
		html = "<![CDATA[" + html + "]]" + ">"

		result = u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}"
							top="{top}" left="{left}" width="{width}" height="{height}">
						<svg>
							<rect x="0" y="0" width="{width}" height="{height}" fill="#EEEEEE"/>
						</svg>{contents}
						<text textalign="right" fontweight="bold" top="{ttop}" left="{tleft}" color="#aaaaaa">Object View ({len})</text>
						<htmltext top="2" left="2" width="{hwidth}" height="{hheight}">{html}</htmltext>
					</container>
				""".format(id = self.id, vis = self.visible, zind = self.zindex, hierarchy = self.hierarchy, order = self.order,
						top = self.top, left = self.left, width = self.width, height = self.height,
						contents = "" #contents
						,tleft   = int(self.width) - 130
						,ttop    = int(self.height) - 24
						,hwidth  = int(self.width) - 130
						,hheight = int(self.height) - 24
						,html    = html
						,len     = datalen
					)

		return VDOM_object.wysiwyg(self, parent, contents=result)

def set_attr(app_id, object_id, param):
	if "pattern" in param:
		src_old = param["pattern"]["old_value"]
		src_new = param["pattern"]["value"]
		import xml
		obj_old = application.objects.search(src_old)	# try to find old and new source objects
		obj_new = application.objects.search(src_new)
		if obj_old:
			obj_old.copy_objects.remove(object_id)
		if obj_new:
			obj_new.copy_objects.append(object_id)
	return ""

