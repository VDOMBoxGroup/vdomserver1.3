import src.util
		
class VDOM_formimage(VDOM_object):

	def render(self, parent, contents=""):
		link = src.util.id.id2link1(self.value)
		id_out = 'o_' + (self.id).replace('-', '_')
		styleblock = ""
		scriptblock = ""
		style = "border:0px;z-index:%s;position:%s;top:%spx;left:%spx;width:%spx;height:%spx;cursor:pointer;"%(self.zindex, self.position, self.top, self.left, self.width, self.height)
		if self.ishover == "1":
			if self.classname == "":
				self.classname = "imagebutton"
			self.classname += "_"+(self.id).replace('-', '_')
			classtut = self.classname
			if self.ispressed == "1":
				self.classname += " " + self.classname + "_sel"
			styleblock = "<style>."+classtut+",."+classtut+"_sel{background:url(\'"+link+"\') left top no-repeat;}."+classtut+"_sel{background-position:left -"+self.height+"px!important}</style>"
			scriptblock = "<script type=\"text/javascript\">jQuery(\"#"+id_out+"\").hover(function(){jQuery(this).addClass('"+self.classname+"_sel');},function(){jQuery(this).removeClass('"+self.classname+"_sel');});jQuery(\"#"+id_out+"\").click(function(){jQuery(this).toggleClass('"+self.classname+"_sel');});</script>"
			link = '/2ca0f428-ffc2-ca5b-3337-610d2384821f.gif'
		if self.classname:
			linkclass = " class=\""+self.classname+"\""
		else:
			linkclass = ""
		if self.visible == "0":
			style += "display:none;"
		result = """<input value='&nbsp;' id="%s" name="%s" style="%s" tabindex="%s" src="%s" border="0" type="image" %s />%s%s"""%(id_out, self.name, style,self.tabindex, link, linkclass, styleblock, scriptblock)
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		image_id = self.value
		image_width = outer_width = self.width
		image_height = outer_height = self.height
		editable = "editable=\"value\""
		
		if "" == image_id:
			image_id = "ef86150b-1aec-469f-a353-18b4751095dc"
			outer_width = outer_height = image_width = image_height = 58
			editable = ""
		result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
					"<svg>"\
						"<image x=\"%s\" y=\"%s\" width=\"%s\" height=\"%s\" href=\"#Res(%s)\" %s/>"\
					"</svg>%s"\
				"</container>"%(
					self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, outer_width, outer_height,
					0, 0, image_width, image_height, image_id, editable, 
					contents)
		
		return VDOM_object.wysiwyg(self, parent, contents=result)

def set_attr(app_id, object_id, param):
	import src.xml
	o = src.xml.xml_manager.search_object(app_id, object_id)
		
	if "value" in param and not "width" in param and not "height" in param:
		attr = param["value"]
		res_id = attr["value"]

		import src.resource
		ro = src.resource.resource_manager.get_resource(app_id, res_id)
		if not ro:
			return "Resource not found"

		# get image resource, obtain width and height and set width and height of the object
		from PIL import Image
		import cStringIO
		s = cStringIO.StringIO()
		s.write(ro.get_data())
		s.seek(0, 0)
		im = Image.open(s)	
		width, height = im.size

		# set attributes
		if o.attributes.width != width or o.attributes.height != height:
			o.set_attributes({"width": width, "height": height})
	else:
		if "width" in param and param["width"]["value"] != '' and int(param["width"]["value"]) > 2500:
			o.set_attributes({"width": 2500})
		if "height" in param and param["height"]["value"] != '' and int(param["height"]["value"]) > 2500:
			o.set_attributes({"height": 2500})
	return ""
	
