<script language='javascript' type='text/javascript'>
function vdom_ui_timerStartOnce(ob, interval)
{
	eval("var obj = Obj_" + ob.substring(2, ob.length) + ";");
	if (typeof obj === 'undefined') return;
	if (interval == undefined) return;/*interval = 500;*/
	$q(obj).stopTime();
	obj.timerfreeze = 0;
	$q(obj).oneTime(interval, function(i){
		if (parseInt(obj.timerfreeze) == 0) {
console.info('vdom_ui_timerStartOnce');
console.info(ob);
			execEventBinded(ob.substring(2, ob.length), 'timechange', {count: i});
/*
			var vdomEvent = new VDOM_Event();
			vdomEvent.srcObj = obj.domCont;
			vdomEvent.name = 'timechange';
			vdomEvent.addParameter('count', i);
			obj.execEvent(vdomEvent);
*/
		}
	});
}
function vdom_ui_timerStart2(ob, interval)
{
	eval("var obj = Obj_" + ob.substring(2, ob.length) + ";");
	if (typeof obj === 'undefined') return;
	if (interval == undefined) return;/*interval = 500;*/
	$q(obj).stopTime();
	obj.timerfreeze = 0;
	$q(obj).everyTime(interval, function(i){
		if (parseInt(obj.timerfreeze) == 0) {
console.info('vdom_ui_timerStart2');
			execEventBinded(ob.substring(2, ob.length), 'timechange', {count: i});
		}
	});
}
</script>
