  <SourceCode><![CDATA[

import datetime

class VDOM_time(VDOM_object):

	def render(self, parent, contents=""):
		result = u''
		id = (self.id).replace('-', '_')
		oid = 'o_' + id
		if self.active == '1':
			if self.once == '1':
				jstartscript = """vdom_ui_timerStartOnce("%s", %s);""" % ( oid, self.interval )
			else:
				jstartscript = """vdom_ui_timerStart2("%s", %s);""" % ( oid, self.interval )
			jstart = """<script language='javascript' type='text/javascript'> jQuery(function($){ $(function($){ %s }); }); </script>""" % ( jstartscript )
		else:
			jstart = ''
		result += """<div id="%s" style="display:none">%s</div>""" % ( oid, jstart )
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		result = """<container id="%s" visible="0" hierarchy="%s" order="%s" top="%s" left="%s" width="42" height="42"><svg><circle cx="20" cy="20" r="19" fill="#ffffff" stroke="#777777" stroke-dasharray="null"/><line x1="20" y1="2" x2="20" y2="19" stroke="#000000" stroke-width="2" stroke-dasharray="null" fill="none"/><line x1="20" y1="19" x2="30" y2="13" stroke="#000000" stroke-width="2" stroke-dasharray="null" fill="none"/><line x1="2" y1="19" x2="6" y2="19" stroke="#000000" stroke-width="2" stroke-dasharray="null" fill="none"/><line x1="34" y1="19" x2="38" y2="19" stroke="#000000" stroke-width="2" stroke-dasharray="null" fill="none"/><line x1="20" y1="34" x2="20" y2="38" stroke="#000000" stroke-width="2" stroke-dasharray="null" fill="none"/></svg>%s</container>"""%(self.id, self.hierarchy, self.order, self.top, self.left, contents)
		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]></SourceCode>
