import datetime

class VDOM_time(VDOM_object):

	def render(self, parent, contents=""):
		result = u''
		id = (self.id).replace('-', '_')
		oid = 'o_' + id
		if self.active == '1':
			if self.once == '1':
				jstartscript = """vdom_ui_timerStartOnce("%s", %s);""" % ( oid, self.interval )
			else:
				jstartscript = """vdom_ui_timerStart2("%s", %s);""" % ( oid, self.interval )
			jstart = """<script language='javascript' type='text/javascript'> $j(function(){ $j(function(){ %s }); }); </script>""" % ( jstartscript )
		else:
			jstart = ''
		result += """<div id="%s" style="display:none">%s</div>""" % ( oid, jstart )
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		import utils.wysiwyg
		
		self.zindex = "0"
		self.width = "50"
		self.height = "50"
		
		image_id = "309f7d7b-b8d7-eb70-46a6-9abbb0ae0cc4"
		result = utils.wysiwyg.get_empty_wysiwyg_value(self, image_id, 0.4)
		
		return VDOM_object.wysiwyg(self, parent, contents=result)]]></SourceCode>
