import json

import e2vdom
import utils.wysiwyg as utils_wysiwyg
from memory import dynobject
from memory.xml_object import xml_object


class VDOM_DynamicObject(VDOM_object):

    def wysiwyg(self, parent, contents=""):
        image_id = "76bfc87a-dbe3-46e3-8d11-cc78a576b63a"
        result = utils_wysiwyg.get_empty_wysiwyg_value(self, image_id)
        return VDOM_object.wysiwyg(self, parent, contents=result)

    def types_on_page(self):
      if request.render_type == "e2vdom":
        return []

      page = application.objects.search(request.container.id)
      if page:
        return page.types.keys()

      return []

    def already_loaded_types(self):
      # return request.shared_variables["vdom.server.types.loaded"] or self.types_on_page()
      return self.types_on_page()

    def set_loaded_types(self, types):
      pass
      # response.shared_variables["vdom.server.types.loaded"] = types

    def load_new_types(self, types):
      # list of types ids
      existing_types = self.already_loaded_types()

      # output data
      result = []

      # iterate over not loaded types
      for tid in list(set(types.keys()) - set(existing_types)):
        libs = types[tid].lib
        if 'html' in libs:
          result.extend(libs['html'])

      # update information on client
      # raise Exception(list(set(types.keys()) | set(existing_types)), types.keys(), existing_types)
      # raise Exception(list(set(types.keys()) | set(existing_types)))
      self.set_loaded_types(list(set(types.keys()) | set(existing_types)))

      # return """<script type="text/javascript">$("head").append({});</script>""".format(json.dumps("\n".join(result)))
      return "\n".join(result)

    def compile_style(self):

        overflow = {
            "1": "hidden",
            "2": "scroll",
            "3": "visible",
        }
        return u"""{display}z-index: {zindex};position: {pos};top: {top}px;left: {left}px;height: {height}px;width: {width}px;overflow: {overflow};""".format(
            display=u"display:none;" if self.visible == "0" else u"",
            zindex=self.zindex,
            pos=self.position,
            top=self.top,
            left=self.left,
            width=self.width,
            height=self.height,
            overflow=overflow.get(self.overflow, "auto"),
        )

    def render_dynamic_content(self, actions):
        xmlobject = xml_object(srcdata=self.vdomxml.encode("utf8"))
        rootobj, objects = dynobject.create_dummy_objects(xmlobject)

        vdomactions = {}
        if actions:
            for event, actions in actions.items():
                try:
                    src_name, src_event = event.split(":")
                except:
                    raise Exception("Invalid event declaration - %s" % event)

                obj = rootobj.find_child_by_name(src_name)
                if not obj:
                    raise Exception("Can't find object by name - %s" % src_name)

                vdomactions[(obj.id, src_event)] = actions_list = []

                for action in actions:
                    if isinstance(action, list):
                        try:
                            dst_name, dst_act = action[0].split(":")
                        except:
                            raise Exception("Invalid action declaration - %s" % action[0])
                        
                        obj = rootobj.find_child_by_name(dst_name)
                        if not obj:
                            raise Exception("Can't find object by name - %s" % dst_name)

                        actions_list.append((obj.id, dst_act) + tuple(action[1:]))

                    else:
                        actions_list.append((self.id, "catchEvent", r'"' + action + r'"', "evt_param(\"evt_ref\")"))


        return u"""%s<script>(function(window, $) {%s})(window, jQuery);</script>%s""" % (
            self.load_new_types(rootobj.all_types()),
            e2vdom.compile_dynamic_e2vdom(rootobj, vdomactions),
            dynobject.dynamic_render(rootobj, objects),
        )

    def render(self, parent, contents="", raw_content=False):

        try:
            vdomactions = json.loads(self.vdomactions)

        except Exception:
            if self.vdomactions:
                raise Exception("Can't parse 'vdomactions'")

            vdomactions = {}

        is_debug = self.debugmode == "1"
        error_text = ""
        dynamic_contents = ""

        if self.vdomxml:
            try:
                dynamic_contents = self.render_dynamic_content(vdomactions)

            except Exception as ex:
                if is_debug:
                    raise

                error_text = u"Render error: {}".format(ex)

        if error_text:
            dynamic_contents = u"""<span class="render-error">{}</span>""".format(self.rendererrormsg or error_text)

        if raw_content:
            return dynamic_contents

        result = u"""<div name="{name}" id="{dom_id}" style="{obj_style}" class="{classes}">{contents}</div>"""
        contents += result.format(
            name=self.name,
            dom_id=u"o_" + self.id.replace('-', '_'),
            obj_style=self.compile_style(),
            classes=self.classname,
            contents=dynamic_contents
        )

        if request.render_type != "e2vdom":
          self.set_loaded_types(self.types_on_page())

        return VDOM_object.render(self, parent, contents=contents)

