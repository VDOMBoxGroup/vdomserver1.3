	<SourceCode>
	<![CDATA[

import re
	
class VDOM_hypertext(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":
			id = 'o_' + (self.id).replace('-', '_')
			if self.nostyle == "2":
				return "%s"%(self.htmlcode)
			elif self.nostyle == "1":
				return "<span id=\"%s\">%s</span>"%(id, self.htmlcode)
			else:
				if self.overflow == "1":
					ov = 'hidden'
				elif self.overflow == "2":
					ov = 'scroll'
				elif self.overflow == "3":
					ov = 'visible'
				else:
					ov = 'auto'
				style="overflow:"+ov+"; position: absolute; z-index: "+self.zindex+"; top: "+self.top+"px; left: "+self.left+"px; width: "+self.width+"px; height: "+self.height+"px"
				return "<div id=\"%s\" style=\"%s\">%s</div>"%(id, style, self.htmlcode)
		else:
			return ""

	def wysiwyg(self, parent, contents=""):
		html_with_server = re.sub("=/", "=http://" + request.server.host + "/"  ,self.htmlcode)
		html_wys = re.sub("(href\s*=\s*['\"]{1}(.*?)['\"]{1})", r"""style="text-decoration: underline; color:#394fa2;" """,html_with_server)
		html_wys = re.sub("<script\s*\>", "<pre>Script:<br/>" ,self.htmlcode)
		html_wys = re.sub("</script>", "</pre>" ,self.htmlcode)
		result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\">"\
				"<htmltext top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">%s</htmltext>"\
				"%s"\
				"<svg>"\
					"<rect opacity=\"0.5\" x=\"0\" y=\"0\" width=\"70\" height=\"20\" fill=\"#FFFFCC\" stroke=\"#FFFF33\"/>"\
				"</svg>"\
				"<text top=\"2\" left=\"5\" width=\"70\" color=\"#FF0000\">Hypertext</text>"\
				"</container>"%(
					self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, 
					0, 0, self.width, self.height, "<![CD"+"ATA["+ html_wys +"]"+"]>", 
					contents)
		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]>
	</SourceCode>
