import e2vdom

def on_compile(application_object, object, action_name, context, objects):
	result = objects
	for xobject in object.get_objects_list():
		result.append({"object": xobject})
	return result

class VDOM_vdomclass(VDOM_object):

	def render(self, parent, contents = ""):
		#e2vdom.process(self, parent)
		#libraries, declarations, registrations = e2vdom.generate(self)
		#clean_id = (application.id).replace('-', '_')
		
		if self.classname == "":
			#classname = ""
			classname = " class='%s' " % (self.classauto)
		else:
			classname = " class='%s %s' " % (self.classauto, self.classname)
		
		style = "position:relative;width:{width}px;height:{height}px;".format(
			width  = self.width, 
			height = self.height
		)

		if self.visible == '1':
			style += "display:block;"
		else:
			style += "display:none;"

		dataid = ""
		if self.dataid != '':
			dataid = "dataid=\"%s\"" % (self.dataid).replace("\"", "")

		if style != "":
			style = " style='%s' " % style
		#result = u"<div {classname} {style}>{contents} DATA: {data}</div>".format(
		result = u"<div objtype='vdomclass' objname='{name}' {classname} {style} {dataid}>{contents}</div>".format(
			#data      = self.data,
			name      = self.name,
			dataid    = dataid,
			classname = classname,
			style     = style,
			contents  = contents
		)

		return VDOM_object.render(self, parent, contents = result)

	def wysiwyg(self, parent, contents = ""):
		result =u"""<container id="{id}" visible="{vis}" zindex="{zind}" hierarchy="{hierarchy}" order="{order}"
						top="{top}" left="{left}" width="{width}" height="{height}">
<svg>
	<rect x="0" y="0" width="{width}" height="{height}" fill="#EEEEEE" />
</svg>
<text textalign="right" fontweight="bold" top="{ttop}" left="{tleft}" color="#aaaaaa">VDOMClass</text>
						{contents}
					</container>
				""".format(	id = self.id, vis = self.visible, zind = self.zindex, hierarchy = self.hierarchy, order = self.order,
							top = self.top, left = self.left, width = self.width, height = self.height, 
							tleft = int(self.width) - 130, ttop = int(self.height) - 24, 
							contents = contents)

		return VDOM_object.wysiwyg(self, parent, contents = result)

