import utils

class VDOM_image(VDOM_object):

	def render(self, parent, contents=""):
		type = ""
		if self.visible == "0":
			type = "display:none; "
			
#		link=src.util.id.id2link(self.value)
		link=utils.id.id2link1(self.value)
		style="z-index: %s;position: %s;top: %spx;left: %spx"%(self.zindex, self.position, self.top, self.left)
		id = 'o_' + (self.id).replace('-', '_')
		title = iw = ih = ""
		if self.hint:
			title = 'title="%s"' % self.hint
		if self.width == "":
			self.width = 0
		if int(self.width) > 0:
			iw = 'width="%s"' % self.width
		if self.height == "":
			self.height = 0
		if int(self.height) > 0:
			ih = 'height="%s"' % self.height
		result="<div id=\"%s\" style=\" %s %s\"><img src=\"%s\" %s %s tabindex=\"%s\" %s></div>"%(id, type, style, link, iw, ih, self.tabindex, title)
		
		
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
	
		image_id = self.value
		image_width = outer_width = self.width
		image_height = outer_height = self.height
		editable = "editable=\"value\""
		
		if "" == image_id:
			image_id = "854dd513-b296-46cd-b0eb-df9d5c86a2d5"
			outer_width = outer_height = image_width = image_height = 58
			editable = ""
		result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
					"<svg>"\
						"<image x=\"%s\" y=\"%s\" width=\"%s\" height=\"%s\" href=\"#Res(%s)\" %s/>"\
					"</svg>%s"\
				"</container>"%(
					self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, outer_width, outer_height,
					0, 0, image_width, image_height, image_id, editable, 
					contents)
		
		return VDOM_object.wysiwyg(self, parent, contents=result)

def set_attr(app_id, object_id, param):
	o = application.objects.search(object_id)
		
	if "value" in param and not "width" in param and not "height" in param:
		attr = param["value"]
		res_id = attr["value"]

		ro = application.resources.get(res_id)
		if not ro:
			return "Resource not found"

		# get image resource, obtain width and height and set width and height of the object
		from PIL import Image
		import cStringIO
		s = cStringIO.StringIO()
		s.write(ro.get_data())
		s.seek(0, 0)
		im = Image.open(s)	
		width, height = im.size

		# set attributes
		if o.attributes.width != width or o.attributes.height != height:
			o.set_attributes({"width": width, "height": height})
	else:
		if "width" in param and param["width"]["value"] != '' and int(param["width"]["value"]) > 2500:
			o.set_attributes({"width": 2500})
		if "height" in param and param["height"]["value"] != '' and int(param["height"]["value"]) > 2500:
			o.set_attributes({"height": 2500})
	return ""

