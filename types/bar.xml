	<SourceCode>
		<![CDATA[

import sys

class VDOM_bar(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":
			vis=""
		else:
			vis="display:none;"
#		if self.visible == "1":
		style="%s z-index: %s;position: %s;top: %spx;left: %spx;width: %spx;height: %spx; background: #%s"%(vis, self.zindex, self.position, self.top, self.left, self.width, self.height, self.color)
		id = 'o_' + (self.id).replace('-', '_')
		result="<div id=\"%s\" style=\"%s\">%s</div>"%(id, style, contents)
#		else:
#			result=""
		return VDOM_object.render(self, parent, contents=result)

	def wysiwyg(self, parent, contents=""):
		result=""\
			"<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
				"<svg>"\
					"<rect x=\"%s\" y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#%s\"/>"\
				"</svg>"\
				"%s"\
			"</container>"%(
				self.id, self.visible, self.zindex, self.hierarchy, self.order,	self.top, self.left, self.width, self.height,
				0, 0, self.width, self.height, self.color,
				contents)
		return VDOM_object.wysiwyg(self, parent, contents=result)

		]]>
	</SourceCode>
