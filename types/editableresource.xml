class VDOM_editableresource(VDOM_object):

	def render(self, parent, contents=""):
		if self.visible == "1":
			resource = application.resources.get_by_label(self.id, "userdata")
			if resource:
				res_id = resource.id
			else:
				res_type = "js" if self.datatype == '1' else "css" if self.datatype == '2' else "txt"
				res_id = application.resources.create_temporary(self.id, "userdata", self.userdata, res_type, "userdata")

			udata = ""
			if self.datatype == "1":
				if res_id:
					udata = u'<script type="text/javascript" src="/%s.res"></script>' % res_id 
				else:
					udata = '<!-- no res for script -->'
			elif self.datatype == "2":
				# first line of the css must be: @charset "UTF-8";
				if res_id:
					udata = u'<style rel="stylesheet" type="text/css">@import url("/%s.css");</style>' % res_id
				else:
					udata = '<!-- no res for style -->'
			else:
				udata = self.userdata

			id = 'o_' + (self.id).replace('-', '_')
			if self.nostyle == "2":
				return u"%s" % udata
			elif self.nostyle == "1":
				return u"<span id=\"%(id)s\">%(data)s</span>" % { "id": id, "data": udata }
			else:
				if self.overflow == "1":
					ov = 'hidden'
				elif self.overflow == "2":
					ov = 'scroll'
				elif self.overflow == "3":
					ov = 'visible'
				else:
					ov = 'auto'
				style = "overflow:"+ov+"; position: absolute; z-index: "+self.zindex+"; top: "+self.top+"px; left: "+self.left+"px; width: "+self.width+"px; height: "+self.height+"px"
				return u"<div id=\"%(id)s\" style=\"%(style)s\">%(data)s</div>"%{"id":id, "style":style, "data":udata}
		else:
			return ""

	def wysiwyg(self, parent, contents=""):
		t = "js" if self.datatype == '1' else "css" if self.datatype == '2' else "txt"
		result="<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\">"\
				"<htmltext top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">%s</htmltext>"\
				"%s"\
				"<svg>"\
					"<rect opacity=\"0.5\" x=\"0\" y=\"0\" width=\"150\" height=\"20\" fill=\"#FFFFCC\" stroke=\"#FFFF33\"/>"\
				"</svg>"\
				"<text top=\"2\" left=\"5\" width=\"150\" color=\"#FF0000\">Editable Resource (%s)</text>"\
				"</container>" % (
					self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, 
					0, 0, self.width, self.height, "", contents, t)
		return VDOM_object.wysiwyg(self, parent, contents=result)

			]]></SourceCode>
