  <SourceCode><![CDATA[
import e2vdom
import xappy
from config import VDOM_CONFIG

class VDOM_indexconnector(VDOM_object):

	# create index with name and parameters entered by user (parametrs may be: 'text', 'date' or  'float')
	def create_index(self, name, param="text"):
		iconn = self.get_iconn()
		iconn.add_field_action(name, xappy.FieldActions.INDEX_FREETEXT)
		iconn.add_field_action(name, xappy.FieldActions.SORTABLE)
		iconn.add_field_action(name, xappy.FieldActions.STORE_CONTENT)
		iconn.close()
		
	# information about field which exist inside index
	def fields_information(self):
		iconn = self.get_iconn()
		l = iconn.get_fields_with_actions()
		iconn.close()
		return l
	
	#information about field for dropdown list
	def fields_list(self):
		fields=""
		iconn = self.get_iconn()
		l = iconn.get_fields_with_actions()
		for r in l:
			fields=fields+"|"+r
		iconn.close()
		return fields
	
	
	
	# add document without metadata 
	def add_file_by_default(self, document):
		iconn=self.get_iconn()
		doc = xappy.UnprocessedDocument()
		for docfield in document:
			if docfield=="category":
				doc.fields.append(xappy.Field(docfield, document[docfield]))
			elif docfield=="db_id":
				doc.fields.append(xappy.Field(docfield, document[docfield]))
			elif docfield=="text":
				doc.fields.append(xappy.Field(docfield, document[docfield]))
			else:
				raise Exception( "Index by default has no field "+docfield+" ")
		pdoc = iconn.process(doc)
		iconn.add(pdoc)
		iconn.flush()
		iconn.close()
		return '1'
	
	# method for checking index fields
	def checking_metadata(self, meta):
		exist_index = self.fields_information()
		for metainf in meta:
			if metainf  in exist_index:
				return True
		return False 
	
	def checking_fields_by_default(self, dat):
		cot=0 	
		for d in dat:
			if d=='category' or d=='db_id'  or d=='text':
				cot=cot+1
		if cot!=3:
			return False
			raise Exception("Document has no information for default fields")
		else:
			return True
	# delete document by id		
	def delete(self, id):
		iconn=self.get_iconn()
				# add document
		iconn.delete(id)
		iconn.flush()
		iconn.close()
		pass
	
	# add document with metadata, but not index
	def add_file_with_metadata(self, docinf):
		docinf_key=docinf.keys()
		if self.checking_metadata(docinf_key):
			if self.checking_fields_by_default(docinf):
				iconn= self.get_iconn()
				# add document
				doc = xappy.UnprocessedDocument()
				for docinformation in docinf:
					doc.fields.append(xappy.Field(docinformation, docinf[docinformation]))
				pdoc = iconn.process(doc)
				iconn.add(pdoc)
				iconn.flush()
				iconn.close()
				return True
			else:	
				raise Exception("Document has no information for default fields")
		else:	
			raise Exception("Document has no: "+docinf_key+ " index")
	
	def delete_all(self):
		import os
		if self.indexid: 
			path=VDOM_CONFIG["TEMP-DIRECTORY"]+"/"+str(self.indexid)
			if os.path.exists(path):
				import shutil
				shutil.rmtree(path)
				self.get_iconn()
			else:
				raise Exception("Please set 'indexid' property")
					
	def get_iconn(self):
		import os
		
		if self.indexid: 
			path=VDOM_CONFIG["TEMP-DIRECTORY"]+"/"+str(self.indexid)
			
			if not os.path.exists(path):
				iconn = xappy.IndexerConnection(path)
				#create index field 'category' (for	 Space)
				iconn.add_field_action('category', xappy.FieldActions.INDEX_FREETEXT)
				iconn.add_field_action('category', xappy.FieldActions.STORE_CONTENT)
				iconn.add_field_action('category', xappy.FieldActions.SORTABLE)
				#create index field 'text' (for document text)
				iconn.add_field_action('text', xappy.FieldActions.INDEX_FREETEXT)
				iconn.add_field_action('text', xappy.FieldActions.STORE_CONTENT)
				iconn.add_field_action('text', xappy.FieldActions.SORTABLE)
				#create index field 'db_id' (for id in data base)
				iconn.add_field_action('db_id', xappy.FieldActions.INDEX_FREETEXT)
				iconn.add_field_action('db_id', xappy.FieldActions.SORTABLE)
				iconn.add_field_action('db_id', xappy.FieldActions.STORE_CONTENT)
				#create index field 'language' (for document language)
				iconn.flush()
				iconn.close()
			
			return xappy.IndexerConnection(path)
		else:
			raise Exception("Please set 'indexid' property")
	
	
	def wysiwyg(self, parent, contents=""):
		return VDOM_object.wysiwyg(self, parent, "")

