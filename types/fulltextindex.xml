  <SourceCode><![CDATA[

import e2vdom
import xappy
from config import VDOM_CONFIG
import os

class VDOM_fulltextindex(VDOM_object):
	
	def render(self, parent, contents=""):
		pass
		#return VDOM_object.render(self, parent, contents="")
	
	def wysiwyg(self, parent, contents=""):
		path=VDOM_CONFIG["TEMP-DIRECTORY"]+"/"+str(self.id)
		if not os.path.exists(path):
			iconn = xappy.IndexerConnection(path)
			#create index field 'category' (for Space)
			iconn.add_field_action('category', xappy.FieldActions.INDEX_FREETEXT)
			iconn.add_field_action('category', xappy.FieldActions.STORE_CONTENT)
			iconn.add_field_action('category', xappy.FieldActions.SORTABLE)
			#create index field 'text' (for document text)
			iconn.add_field_action('text', xappy.FieldActions.INDEX_FREETEXT)
			iconn.add_field_action('text', xappy.FieldActions.STORE_CONTENT)
			iconn.add_field_action('text', xappy.FieldActions.SORTABLE)
			#create index field 'db_id' (for id in data base)
			iconn.add_field_action('db_id', xappy.FieldActions.INDEX_FREETEXT)
			iconn.add_field_action('db_id', xappy.FieldActions.SORTABLE)
			iconn.add_field_action('db_id', xappy.FieldActions.STORE_CONTENT)
			#create index field 'language' (for document language)
			iconn.flush()
			iconn.close()
		return VDOM_object.wysiwyg(self, parent, "")

		]]></SourceCode>
