	<SourceCode>
	<![CDATA[

class VDOM_formdropdown(VDOM_object):

	def render(self, contents=""):
		if self.visible == "1":
			name=self.name
			style="position: absolute; z-index: "+self.zindex+"; top: "+self.top+"px; left: "+self.left+"px; width: "+self.width+"px; height: "+self.height+"px"
			style+="; font: 14px tahoma"
			id = 'o_' + (self.id).replace('-', '_')
			if self.size and self.size != "":
				size_attr = " size=\"%s\""%self.size
			else:
				size_attr = ""

			if self.classname == "":
				classname = ""
			else:
				classname = "class='"+self.classname+"'"

			if self.multiselect and self.multiselect == "1":
				multiselect_attr = " multiselect=\"1\""
			else:
				multiselect_attr = ""
			result="<select id=\"%s\" name=\"%s\" tabindex=\"%s\"%s%s style=\"%s\" %s>"%(id, name, self.tabindex,size_attr,multiselect_attr, style, classname)
			if not self.value:
				self.value = self.value1
			else:
				#debug("?????value1: "  + self.value1);
				pass

			list=self.value.split("|")
			list1=self.value1.split("|")
			if len(list) != len(list1): return ""
			for idx in xrange(len(list1)):
				if unicode(list1[idx]) == unicode(self.selectedvalue):
					selected = "selected=\"selected\" "
				else:
					selected = ""
				result+="<option value=\"" + list1[idx] + "\" %s>"%(selected) + list[idx] + "</option>"
			result+="</select>"
			return result
		else:
			return ""

	def wysiwyg(self, parent, contents=""):

		result = "<container id=\"%s\" visible=\"%s\" zindex=\"%s\" hierarchy=\"%s\" order=\"%s\" top=\"%s\" left=\"%s\" width=\"%s\" height=\"%s\">"\
			"<svg>"\
				"<rect y=\"%s\" x=\"%s\" width=\"%s\" height=\"%s\" fill=\"#FFFFFF\" stroke=\"#000000\"/>"\
			"</svg>%s"%(
				self.id, self.visible, self.zindex, self.hierarchy, self.order, self.top, self.left, self.width, self.height,
				0, 0, int(self.width)-1, int(self.height)-1, contents)

		result += ""\
			"<svg>"\
				"<rect x=\"%s\" y=\"%s\" width=\"%s\" height=\"%s\" fill=\"#EEEEEE\" stroke=\"#000000\"/>"\
				"<polygon fill=\"black\" stroke=\"#000000\" points=\"%s,%s %s,%s %s,%s\"/>"\
			"</svg>"%(
				int(self.width)-14, 0, 14, int(self.height)-1,
				int(self.width)-9, int(self.height) / 2 - 3,
				int(self.width)-5, int(self.height) / 2 - 3,
				int(self.width)-7, int(self.height) / 2 + 3	)
		result += "</container>"

		return VDOM_object.wysiwyg(self, parent, contents=result)

	]]>
